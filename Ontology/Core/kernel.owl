                                                <swrl:classPredicate rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#PhysicalObject"/>
                                                <swrl:argument1 rdf:resource="urn:swrl#x"/>
