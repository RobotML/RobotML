                                                                <swrl:argument2 rdf:resource="urn:swrl#a"/>
                                                                <swrl:argument1 rdf:resource="urn:swrl#x"/>
                                                            </rdf:Description>
                                                        </rdf:first>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
