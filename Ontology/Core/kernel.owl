                    <rdf:Description>
                        <rdf:type rdf:resource="&swrl;AtomList"/>
                        <rdf:rest rdf:resource="&rdf;nil"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="&swrl;IndividualPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#constrainsFrame"/>
                                <swrl:argument2 rdf:resource="urn:swrl#a"/>
                                <swrl:argument1 rdf:resource="urn:swrl#b"/>
                            </rdf:Description>
                        </rdf:first>
                    </rdf:Description>
                </rdf:rest>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="&swrl;IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#constrainsFrame"/>
                        <swrl:argument1 rdf:resource="urn:swrl#a"/>
                        <swrl:argument2 rdf:resource="urn:swrl#b"/>
