                                                                    </rdf:Description>
                                                                </rdf:first>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                        <rdf:first>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="&swrl;IndividualPropertyAtom"/>
