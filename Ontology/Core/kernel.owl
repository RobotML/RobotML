    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&swrl;Imp"/>
        <rdfs:comment xml:lang="en">this rule enforces the fact that if a physical object triggers an  action then it is an agent.</rdfs:comment>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="&swrl;AtomList"/>
                <rdf:rest rdf:resource="&rdf;nil"/>
