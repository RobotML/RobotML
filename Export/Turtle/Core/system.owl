#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#TODO

kern:TODO rdf:type owl:AnnotationProperty .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#author

kern:author rdf:type owl:AnnotationProperty .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#openCycURI

kern:openCycURI rdf:type owl:AnnotationProperty .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#wikipediaURI

kern:wikipediaURI rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#Timeframes

:Timeframes rdf:type owl:ObjectProperty ;
            
            rdfs:comment "Clocks are providing timeframe to hardware"@en ;
            
            rdfs:domain kern:Clock ;
            
            rdfs:subPropertyOf kern:isPropertyOf ;
            
            rdfs:range :ComputationalSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#canBeCodedInMessage

:canBeCodedInMessage rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment ""@en ;
                     
                     rdfs:range :MiddlewareMessage ;
                     
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :RoboticSystemConfigurationData
                                                 :RoboticSystemInfoData
                                               )
                                 ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#canCommunicateThrough

:canCommunicateThrough rdf:type owl:ObjectProperty ;
                       
                       rdfs:comment "An Application can use specific and dedicated applications called Communicatio middleware to communicate"@en ;
                       
                       rdfs:domain kern:Application ;
                       
                       rdfs:subPropertyOf kern:isPropertyOf ;
                       
                       rdfs:range :CommunicationMiddleware ;
                       
                       owl:inverseOf :hasApplicationPool .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#controlsSystem

:controlsSystem rdf:type owl:ObjectProperty ;
                
                rdfs:comment "Interaction between a Control System and a Robotic System in order to achieve the corresponding control process:  regulators will take part to get the final output command to send to the Hardware or software of the robotic system. This property is not mapped anymore on the generic system definition that tackles only"@en ;
                
                rdfs:domain :ControlSystem ;
                
                rdfs:range :DriverDeviceSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasApplicationPool

:hasApplicationPool rdf:type owl:ObjectProperty ;
                    
                    rdfs:range kern:Application ;
                    
                    rdfs:subPropertyOf kern:has ;
                    
                    rdfs:domain :CommunicationMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasClock

:hasClock rdf:type owl:ObjectProperty ;
          
          rdfs:comment "Some hardware are using a specific piece of hardware modeling a timeframe and called a clock"@en ;
          
          rdfs:range kern:Clock ;
          
          rdfs:subPropertyOf kern:has ;
          
          rdfs:domain :ComputationalSystem ;
          
          owl:inverseOf :Timeframes .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasConfigurationData

:hasConfigurationData rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "Each one of the robotic components (actuators, sensors, etc) has some configuration parameter data."@en ;
                      
                      rdfs:subPropertyOf kern:has ;
                      
                      rdfs:domain :DriverDeviceSystem ;
                      
                      rdfs:range :RoboticSystemConfigurationData .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasImplementingHardware

:hasImplementingHardware rdf:type owl:ObjectProperty ;
                         
                         rdfs:comment "Robotic functional system has implementing hardware."@en ;
                         
                         rdfs:subPropertyOf kern:has ;
                         
                         rdfs:domain :RoboticFunctionalSystem ;
                         
                         owl:inverseOf :isHarwareImplementationOf ;
                         
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( :ActuatorHardware
                                                    :CommunicationHardware
                                                    :PowerHardware
                                                    :SensorHardware
                                                  )
                                    ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasImplementingSoftware

:hasImplementingSoftware rdf:type owl:ObjectProperty ;
                         
                         rdfs:comment "Robotic functional system has implementing software."@en ;
                         
                         rdfs:range kern:Software ;
                         
                         rdfs:subPropertyOf kern:has ;
                         
                         rdfs:domain :RoboticFunctionalSystem ;
                         
                         owl:inverseOf :isSoftwareImplementationOf .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasInfoData

:hasInfoData rdf:type owl:ObjectProperty ;
             
             rdfs:subPropertyOf kern:has ;
             
             rdfs:domain :DriverDeviceSystem ;
             
             rdfs:range :RoboticSystemInfoData .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasInput

:hasInput rdf:type owl:ObjectProperty ;
          
          rdfs:comment "A control system or a commandable system can have some command orders as input of a desired action."@en ;
          
          rdfs:subPropertyOf kern:has ;
          
          rdfs:domain :Closed-LoopControlSystem ;
          
          rdfs:range :ControlCommandOrder .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasMethods

:hasMethods rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasNoise

:hasNoise rdf:type owl:ObjectProperty ;
          
          rdfs:comment "A System has to deal with disruptions or perturbations known as Noise to process a signal. These perturbations are inherent in the data adquisition process or in the commandable processes, and they can be present, for instance, in a control system, actuator system, sensor system, etc..."@en ;
          
          rdfs:subPropertyOf kern:has ;
          
          rdfs:domain :Closed-LoopControlSystem ;
          
          rdfs:range :NoiseSignal .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasNoiseModel

:hasNoiseModel rdf:type owl:ObjectProperty ;
               
               rdfs:comment "A distorted interaction due to a noise signal can be modelled by an Evolution model of the corresponding noise."@en ;
               
               rdfs:range kern:NoiseModel ;
               
               rdfs:subPropertyOf kern:has ;
               
               rdfs:domain :NoiseSignal .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasOutput

:hasOutput rdf:type owl:ObjectProperty ;
           
           rdfs:subPropertyOf kern:has ;
           
           rdfs:domain :Closed-LoopControlSystem ;
           
           rdfs:range :ControlCommandOutput .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasinterface

:hasinterface rdf:type owl:ObjectProperty ;
              
              rdfs:comment "A robotic system has an interface to publish or subscribe to its corresponding robotic middleware."@en ;
              
              rdfs:subPropertyOf kern:has ;
              
              rdfs:domain :DriverDeviceSystem ;
              
              rdfs:range :SystemInterface .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hostInteractions

:hostInteractions rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "A RoboticMiddleware is an interoperability software that not only executes applications, send and triggers interactions between systems/components, but it also hosts the interactions of the robotic platform."@en ;
                  
                  rdfs:range kern:ApplicationInteraction ;
                  
                  rdfs:domain :RoboticMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isControlledBy

:isControlledBy rdf:type owl:ObjectProperty ;
                
                rdfs:comment "A robotic system is a controllable system as such there could exists a function (without any concern on how it is implemented) that takes into account the control of this particular robotic system."@en ;
                
                rdfs:range :ControlSystem ;
                
                rdfs:domain :DriverDeviceSystem ;
                
                owl:inverseOf :controlsSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isHarwareImplementationOf

:isHarwareImplementationOf rdf:type owl:ObjectProperty ;
                           
                           rdfs:comment "Hardware is implementation of robotic functional system."@en ;
                           
                           rdfs:subPropertyOf kern:isPropertyOf ;
                           
                           rdfs:range :RoboticFunctionalSystem ;
                           
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :ActuatorHardware
                                                       :CommunicationHardware
                                                       :PowerHardware
                                                       :SensorHardware
                                                     )
                                       ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isProtectedBy

:isProtectedBy rdf:type owl:ObjectProperty ;
               
               rdfs:comment "A robotic system is a controllable system as such there could exists a function (without any concern on how it is implemented) that takes into account the protection of this particular robotic system. Protection means that in case of abnormal behaviour of the robotic system considered, there will be interactions occurring that will allow it to go on working on degraded modes or to stop it completely in order not to be destryed."@en ;
               
               rdfs:domain :DriverDeviceSystem ;
               
               rdfs:range :SecuritySystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isRepresentedBy

:isRepresentedBy rdf:type owl:ObjectProperty ;
                 
                 rdfs:domain kern:State ;
                 
                 owl:inverseOf :isRepresentingA .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isRepresentingA

:isRepresentingA rdf:type owl:ObjectProperty ;
                 
                 rdfs:range kern:State .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isSoftwareImplementationOf

:isSoftwareImplementationOf rdf:type owl:ObjectProperty ;
                            
                            rdfs:comment "Software is implementation of robotic functionnal system."@en ;
                            
                            rdfs:domain kern:Software ;
                            
                            rdfs:subPropertyOf kern:isPropertyOf ;
                            
                            rdfs:range :RoboticFunctionalSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#managesMiddlewareMessage

:managesMiddlewareMessage rdf:type owl:ObjectProperty ;
                          
                          rdfs:range :MiddlewareMessage ;
                          
                          rdfs:domain :SystemInterface .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#mapsOnto

:mapsOnto rdf:type owl:ObjectProperty ;
          
          rdfs:comment "A robotic functional system in order to exist must be mapped onto an existing robotic system. Either it is mapped through software or hardware implementation or a mix is not described into this property. Two robotic systems can be different but implement the saame functional system."@en ;
          
          rdfs:range :DriverDeviceSystem ;
          
          rdfs:domain :RoboticFunctionalSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#measuresOutput

:measuresOutput rdf:type owl:ObjectProperty ;
                
                rdfs:range :ControlCommandOutput ;
                
                rdfs:domain :ProprioceptiveSensorSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#protectsSystem

:protectsSystem rdf:type owl:ObjectProperty ;
                
                rdfs:comment "Interaction between a Security System and a Robotic System (in general). This interaction guarantees the protection in different levels (network, criptography, control, etc...) of a robotic system. This property does not concern the generic system concept that tackles only with composition and interefacing properties."@en ;
                
                rdfs:range :DriverDeviceSystem ;
                
                rdfs:domain :SecuritySystem ;
                
                owl:inverseOf :isProtectedBy .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#publish

:publish rdf:type owl:ObjectProperty ;
         
         rdfs:comment "The output interface controller manages the publishment of dataflow from a robotic system to the corresponding robotic middleware."@en ;
         
         rdfs:domain :OutputSystemInterface ;
         
         rdfs:range :RoboticMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#subscribe

:subscribe rdf:type owl:ObjectProperty ;
           
           rdfs:domain :InputSystemInterface ;
           
           rdfs:range :RoboticMiddleware .





#################################################################
#
#    Data properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#DegreesOfFreedom

:DegreesOfFreedom rdf:type owl:DatatypeProperty ;
                  
                  kern:wikipediaURI "http://en.wikipedia.org/wiki/Degrees_of_freedom"^^xsd:anyURI ;
                  
                  rdfs:comment "In many scientific fields, the degrees of freedom of a system is the number of parameters of the system that may vary independently. For example, the position of a figure in the plane has three degrees of freedom: its orientation and the two coordinates of any fixed point of the figure."@en ;
                  
                  rdfs:domain :Joint ;
                  
                  rdfs:range xsd:positiveInteger .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#isActive

:isActive rdf:type owl:DatatypeProperty ;
          
          rdfs:comment "null"@en ;
          
          rdfs:domain :SensorSystem ;
          
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:oneOf [ rdf:type rdf:List ;
                                   rdf:first "false"^^xsd:boolean ;
                                   rdf:rest [ rdf:type rdf:List ;
                                              rdf:first "true"^^xsd:boolean ;
                                              rdf:rest rdf:nil
                                            ]
                                 ]
                     ] .





#################################################################
#
#    Classes
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#MachineTorque

<http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#MachineTorque> rdf:type owl:Class .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Action

kern:Action owl:disjointWith :NoiseSignal .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Application

kern:Application owl:disjointWith :SystemInterface ;
                 
                 kern:TODO """Application impacts only software through ApllicationInteraction and Driver that is an Application impacts Physical objects due to hardwaretosoftware and softwaretohardware interaction.

This is clearly a mistake to be repaired."""@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#ApplicationInteraction

kern:ApplicationInteraction rdf:type owl:Class .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Clock

kern:Clock owl:disjointWith :ActuatorHardware ,
                            :CommunicationHardware ,
                            :ControlStationSystem ,
                            :HardwareImplementationOfController ,
                            :PowerHardware ,
                            :SensorHardware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Driver

kern:Driver rdf:type owl:Class ;
            
            owl:disjointWith :RoboticApplication ,
                             :RoboticMiddleware ;
            
            kern:TODO "Driver should triggers interactions between software and physical world (see definition of RoboticSystem)"@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Environment

kern:Environment owl:disjointWith :DriverDeviceSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#FileSystem

kern:FileSystem owl:disjointWith :CommunicationMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Hardware

kern:Hardware rdfs:comment "The robotic System concept provides the tool to aggregate the driber onto its hardware counterpart."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#OperatingSystem

kern:OperatingSystem owl:disjointWith :SystemInterface .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#PhysicalInteraction

kern:PhysicalInteraction rdf:type owl:Class .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Software

kern:Software rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty kern:triggers ;
                                owl:someValuesFrom kern:ApplicationInteraction
                              ] ;
              
              rdfs:comment "In this scope, \"Models\" property means \"Implements\"."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#SoftwareModule

kern:SoftwareModule rdf:type owl:Class ;
                    
                    owl:disjointWith :RoboticApplication ,
                                     :RoboticMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#SoftwareProbe

kern:SoftwareProbe owl:disjointWith :RoboticApplication ,
                                    :RoboticMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Wrapper

kern:Wrapper owl:disjointWith :RoboticApplication ,
                              :RoboticMiddleware .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ActuatedJoint

:ActuatedJoint rdf:type owl:Class ;
               
               rdfs:subClassOf :Joint .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ActuatorConfigurationData

:ActuatorConfigurationData rdf:type owl:Class ;
                           
                           rdfs:subClassOf :RoboticSystemConfigurationData ;
                           
                           owl:disjointWith :SensorConfigurationData ,
                                            :StructuralConfigurationData .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ActuatorDriver

:ActuatorDriver rdf:type owl:Class ;
                
                rdfs:subClassOf kern:Driver ;
                
                owl:disjointWith :CommunicationDriver ,
                                 :PowerDriver ,
                                 :SensorDriver ;
                
                rdfs:comment "Software part of the Actuator System of a mechanical platform which will be composed of drivers to control the effectors like a mechanical arm driver."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ActuatorHardware

:ActuatorHardware rdf:type owl:Class ;
                  
                  rdfs:subClassOf kern:Hardware ;
                  
                  owl:disjointWith :CommunicationHardware ,
                                   :PowerHardware ,
                                   :SensorHardware ;
                  
                  rdfs:comment "the Actuator Hardware is a hardware to be included in a mechanical platform which will be composed of many different parts. Actuator hardware implements the capability to act on environment thanks to energy transformation using for example an engine."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ActuatorInfoData

:ActuatorInfoData rdf:type owl:Class ;
                  
                  rdfs:subClassOf :RoboticSystemInfoData ;
                  
                  owl:disjointWith :SensorInfoData .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ActuatorSystem

:ActuatorSystem rdf:type owl:Class ;
                
                rdfs:subClassOf :DriverDeviceSystem ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty kern:aggregates ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( :ActuatorDriver
                                                                    :ActuatorHardware
                                                                  )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty kern:aggregates ;
                                  owl:someValuesFrom :ActuatorHardware
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty kern:aggregates ;
                                  owl:someValuesFrom :ActuatorDriver
                                ] ;
                
                owl:disjointWith :CommunicationSystem ,
                                 :PowerManagedSystem ,
                                 :SensorSystem ,
                                 :SupportingStructure ;
                
                rdfs:comment """System INCLUDING software which control effectors, like a mechanical arm driver.

IMPORTANT WARNINGS, In the Robotic field different comments are added here:
- When the \"Actuator\" word is used alone, it means the Actuator system includes only one non decomposable software and one non decomposable hardware. This is of great ambiguity but reflects usage in the community.
- There is often a difference between the Motorisation system, able to move the robot or part of it, and the different other systems able to interact with the environment. They can be called Payload systems, Operational systems, Mission modules, etc (as an example, NRBC, Weapons, Communications, ...)"""@en ;
                
                kern:TODO "There are not only electromechanical systems that can be considered as actuators. Hydraulics could be or airflow or batteries..."@en ;
                
                kern:author """creation PROTEUS consortium - 
modified Bruno Patin - 14/09/2013
- comment has been modified to eliminate the reference to PROTEUS project"""@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#AdaptiveController

:AdaptiveController rdf:type owl:Class ;
                    
                    rdfs:subClassOf :Closed-LoopControlSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#Arm

:Arm rdf:type owl:Class ;
     
     rdfs:subClassOf :Manipulator .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#CameleonCommand_msg

