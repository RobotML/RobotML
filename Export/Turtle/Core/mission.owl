@prefix : <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix xsp: <http://www.owl-ontologies.com/2005/08/07/xsp.owl#> .
@prefix envi: <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#> .
@prefix info: <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#> .
@prefix kern: <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#> .
@prefix miss: <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix robo: <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/robot.owl#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix sqwrl: <http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix owl2xml: <http://www.w3.org/2006/12/owl2-xml#> .
@prefix protege: <http://protege.stanford.edu/plugins/owl/protege#> .
@base <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl> .

<http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl> rdf:type owl:Ontology ;
                                                                                               
                                                                                               owl:versionInfo "version delivered 1.0"^^xsd:string ;
                                                                                               
                                                                                               rdfs:comment "FaCT++ does not work on this ontology. Reasoning shall be performed using HermiT."@en ,
                                                                                                            "This ontology describes the mission as seen by a system. It is mainly a part of the information ontology.It is using as its main paradigm for representing the mission, the Hierarchical Tasks Network as can be seen described in Julien Guitton Thesis - \"Architecture Hybride pour la planification d'actions et de déplacements\" - ISAE 2010."@en ,
                                                                                                            "This work is based upon a previous one funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project (see http://www.anr-project.fr). It is the property of its authors and is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files)."@en ,
                                                                                                            "Version for Protégé 4.x flavors."@en ;
                                                                                               
                                                                                               owl:imports <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl> ,
                                                                                                           <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl> ,
                                                                                                           <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl> ,
                                                                                                           <http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#TODO

kern:TODO rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#isAnimatedByEvolution

kern:isAnimatedByEvolution rdf:type owl:ObjectProperty .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#allocatesTask

:allocatesTask rdf:type owl:ObjectProperty ;
               
               rdfs:comment "Distributed task allocation allocates tasks"@en ;
               
               rdfs:domain :DistributedTaskAllocation ;
               
               rdfs:range :Task .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#executesPlan

:executesPlan rdf:type owl:ObjectProperty ;
              
              rdfs:comment "The executive System executes de plan generated by the planning system."@en ;
              
              rdfs:domain :ExecutiveSystem ;
              
              rdfs:range :Plan .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasAPlan

:hasAPlan rdf:type owl:ObjectProperty ;
          
          rdfs:comment "The mission has a plan."@en ;
          
          rdfs:domain :Mission ;
          
          rdfs:range :Plan .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasConstraint

:hasConstraint rdf:type owl:ObjectProperty ;
               
               rdfs:comment "The constraints of a mission define what is not possible or forbidden."@en ;
               
               rdfs:range info:Constraint ;
               
               rdfs:domain :Mission .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasDecomposition

:hasDecomposition rdf:type owl:InverseFunctionalProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:comment "A task has zero to several decompositions i.e. different ways to perform the task."@en ;
                  
                  rdfs:range :Decomposition ;
                  
                  rdfs:domain :Task ;
                  
                  owl:inverseOf :isDecompositionOfTask .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasGoalState

:hasGoalState rdf:type owl:ObjectProperty ;
              
              rdfs:comment "A mission objective may be specified as a state to be reached."@en ;
              
              rdfs:range kern:State ;
              
              rdfs:domain :MissionObjective .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasHighLevelTask

:hasHighLevelTask rdf:type owl:ObjectProperty ;
                  
                  rdfs:domain :MissionObjective ;
                  
                  rdfs:range :Task .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasObjective

:hasObjective rdf:type owl:ObjectProperty ;
              
              rdfs:comment "A mission has objectives."@en ;
              
              rdfs:domain :Mission ;
              
              rdfs:range :MissionObjective .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasPlannedOperator

:hasPlannedOperator rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "A plan always has planned operators."@en ;
                    
                    rdfs:range :Operator ;
                    
                    rdfs:domain :Plan .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasPostCondition

:hasPostCondition rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "An oprator has post conditions indicating how the state is changed by the operator."@en ;
                  
                  rdfs:range kern:Condition ;
                  
                  rdfs:domain :Operator .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasPrecondition

:hasPrecondition rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "A decompostion or an operator has a pre condition indicating that it is allowed."@en ;
                 
                 rdfs:range kern:Condition ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Decomposition
                                             :Operator
                                           )
                             ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasReductionTask

:hasReductionTask rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "A decomposition has reduction tasks."@en ;
                  
                  rdfs:domain :Decomposition ;
                  
                  rdfs:range :Task .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasRessource

:hasRessource rdf:type owl:ObjectProperty ;
              
              rdfs:comment "The ressources of a mission are the agents in charge of performing it."@en ;
              
              rdfs:range kern:Agent ;
              
              rdfs:domain :Mission .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasTaskAllocationProtocol

:hasTaskAllocationProtocol rdf:type owl:ObjectProperty ;
                           
                           rdfs:comment "Distributed task allocation is based on a task allocation protocol."@en ;
                           
                           rdfs:domain :DistributedTaskAllocation ;
                           
                           rdfs:range :TaskAllocationProtocol .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasType

:hasType rdf:type owl:ObjectProperty ;
         
         rdfs:domain :Mission ;
         
         rdfs:range :MissionType .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#hasWorkFlow

:hasWorkFlow rdf:type owl:ObjectProperty ;
             
             rdfs:comment "A mission has a workflow."@en ;
             
             rdfs:domain :Mission ;
             
             rdfs:range :WorkFlow .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#includesConstraint

:includesConstraint rdf:type owl:ObjectProperty ;
                    
                    rdfs:range info:Constraint ;
                    
                    rdfs:domain info:OperationalInformation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#induceActivity

:induceActivity rdf:type owl:ObjectProperty ;
                
                rdfs:comment "An operator, when executed, induces activities."@en ;
                
                rdfs:range kern:Activity ;
                
                rdfs:domain :Operator .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#isBuiltBy

:isBuiltBy rdf:type owl:ObjectProperty ;
           
           rdfs:comment "A plan is build by an agent."@en ;
           
           rdfs:range kern:Agent ;
           
           rdfs:domain :Plan .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#isBuiltFrom

:isBuiltFrom rdf:type owl:ObjectProperty ;
             
             rdfs:domain :Plan ;
             
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( info:Constraint
                                        :MissionObjective
                                      )
                        ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#isDecompositionOfTask

:isDecompositionOfTask rdf:type owl:FunctionalProperty ,
                                owl:ObjectProperty ;
                       
                       rdfs:comment "A decomposition is always the decomposition of a task."@en ;
                       
                       rdfs:domain :Decomposition ;
                       
                       rdfs:range :Task .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#isDescribedBy

:isDescribedBy rdf:type owl:ObjectProperty ;
               
               rdfs:comment "A mission is described by its state machine, its constraints and its tasks. The workflow of the mission is described by the same state machine."@en ;
               
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Mission
                                           :WorkFlow
                                         )
                           ] ;
               
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( info:Constraint
                                          :MissionStateMachine
                                          :Task
                                        )
                          ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#isExpectedToChangeState

:isExpectedToChangeState rdf:type owl:ObjectProperty ;
                         
                         rdfs:comment "An operator is expected to change state."@en ;
                         
                         rdfs:range kern:State ;
                         
                         rdfs:domain :Operator .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#manageMission

:manageMission rdf:type owl:ObjectProperty ;
               
               rdfs:comment "The mission management system generates the plan with the planning system and executes it with the executive system."@en ;
               
               rdfs:range :Mission ;
               
               rdfs:domain <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#MissionManagementSystem> .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#performs

:performs rdf:type owl:ObjectProperty ;
          
          rdfs:comment "An agent performs actions."@en ;
          
          rdfs:domain kern:Agent .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#producesPlan

:producesPlan rdf:type owl:ObjectProperty ;
              
              rdfs:comment "The planning system generates a Plan which would be executed by the executive System."@en ;
              
              rdfs:range :Plan ;
              
              rdfs:domain :PlanningSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#takesIntoAccount

:takesIntoAccount rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "takesIntoAccount"@en ;
                  
                  rdfs:domain :PlanningSystem ;
                  
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( info:Constraint
                                             :MissionObjective
                                           )
                             ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#takesPlaceInto

:takesPlaceInto rdf:type owl:ObjectProperty ;
                
                rdfs:comment "A mission takes place into an environement."@en ;
                
                rdfs:range kern:Environment ;
                
                rdfs:domain :Mission .





#################################################################
#
#    Data properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#isSequencial

:isSequencial rdf:type owl:DatatypeProperty ;
              
              rdfs:comment "If it is not sequencial parallelism is allowed."@en ;
              
              rdfs:domain :Decomposition ;
              
              rdfs:range xsd:boolean .





#################################################################
#
#    Classes
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#OperationalInformation

info:OperationalInformation kern:TODO "The comment expresses two different concepts. They are not particularly congruent."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#CommunicationOfBeliefAboutEnvironment

:CommunicationOfBeliefAboutEnvironment rdf:type owl:Class ;
                                       
                                       rdfs:subClassOf :CommunicationOfBeliefs ;
                                       
                                       owl:disjointWith :CommunicationOfBeliefAboutOtherAgents ,
                                                        :CommunicationOfSelfStateEstimation ;
                                       
                                       rdfs:comment "Estimation of the state of the things such as targets and landmarks and of the agents not performing the mission."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#CommunicationOfBeliefAboutOtherAgents

:CommunicationOfBeliefAboutOtherAgents rdf:type owl:Class ;
                                       
                                       rdfs:subClassOf :CommunicationOfBeliefs ;
                                       
                                       owl:disjointWith :CommunicationOfSelfStateEstimation ;
                                       
                                       rdfs:comment "The belief about others may be expressed with respect to own. For istance relative distance."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#CommunicationOfBeliefs

:CommunicationOfBeliefs rdf:type owl:Class ;
                        
                        rdfs:subClassOf kern:InterAgentCommunication ;
                        
                        owl:disjointWith :CommunicationOfIntents ,
                                         :CoordinationSignal ,
                                         :Negotiation ;
                        
                        rdfs:comment "Exchange of data about belived state of ownship, of other agents and of the world."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#CommunicationOfIntents

:CommunicationOfIntents rdf:type owl:Class ;
                        
                        rdfs:subClassOf kern:InterAgentCommunication ;
                        
                        owl:disjointWith :CoordinationSignal ,
                                         :Negotiation ;
                        
                        rdfs:comment "Communication of own plans."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#CommunicationOfSelfStateEstimation

:CommunicationOfSelfStateEstimation rdf:type owl:Class ;
                                    
                                    rdfs:subClassOf :CommunicationOfBeliefs ;
                                    
                                    rdfs:comment "Corresponds mainly to the broadcast of bevived position and attitude."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#ContractNetProtocol

:ContractNetProtocol rdf:type owl:Class ;
                     
                     rdfs:subClassOf :TaskAllocationProtocol ;
                     
                     owl:disjointWith :FoundationForIntelligentPhysicalAgentsProtocol .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#CoordinationSignal

:CoordinationSignal rdf:type owl:Class ;
                    
                    rdfs:subClassOf kern:InterAgentCommunication ;
                    
                    owl:disjointWith :Negotiation ;
                    
                    rdfs:comment "This kind of communication allows coordination and synchronization at execution time."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#Decomposition

:Decomposition rdf:type owl:Class ;
               
               rdfs:subClassOf info:OperationalInformation ;
               
               owl:disjointWith :Mission ,
                                :MissionObjective ,
                                :MissionType ,
                                :OperationalConstraint ,
                                :Plan ,
                                :Task ,
                                :WorkFlow ;
               
               rdfs:comment "Decomposition of a task."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#DistributedConflictSolving

:DistributedConflictSolving rdf:type owl:Class ;
                            
                            rdfs:subClassOf :Negotiation ;
                            
                            owl:disjointWith :DistributedTaskAllocation ;
                            
                            rdfs:comment "Exchanges for solving conflicts."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#DistributedTaskAllocation

:DistributedTaskAllocation rdf:type owl:Class ;
                           
                           rdfs:subClassOf :Negotiation ;
                           
                           rdfs:comment "Communication for distributed task allocation."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#EnvironmentalConstraint

:EnvironmentalConstraint rdf:type owl:Class ;
                         
                         rdfs:subClassOf info:Constraint ;
                         
                         owl:disjointWith :OperationalConstraint ;
                         
                         rdfs:comment "This type of constraint describes the impacts of the environment on degrees of freedom for the agents involved in the mission."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#ExecutiveSystem

:ExecutiveSystem rdf:type owl:Class ;
                 
                 rdfs:subClassOf <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticFunctionalSystem> ;
                 
                 owl:disjointWith :PlanningSystem ,
                                  :ReactiveSystem ,
                                  <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#ControlSystem> ,
                                  <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#MissionManagementSystem> ,
                                  <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#MotionPlanningSystem> ,
                                  <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#PlatformManagementSystem> ,
                                  <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#SecuritySystem> ;
                 
                 rdfs:comment "This system is able to produce actions on the basis of a plan. It is part of the MissionManagementSystem."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#FiniteStateMachine

:FiniteStateMachine rdf:type owl:Class ;
                    
                    rdfs:subClassOf kern:StateMachine ;
                    
                    kern:TODO "This concept should be moved. It is not linked to Mission ontology but to underlying ontology to decide."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#FoundationForIntelligentPhysicalAgentsProtocol

:FoundationForIntelligentPhysicalAgentsProtocol rdf:type owl:Class ;
                                                
                                                rdfs:subClassOf :TaskAllocationProtocol .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#Mission

:Mission rdf:type owl:Class ;
         
         rdfs:subClassOf info:OperationalInformation ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty kern:isAnimatedByEvolution ;
                           owl:allValuesFrom :MissionStateMachine
                         ] ;
         
         owl:disjointWith :MissionObjective ,
                          :MissionType ,
                          :OperationalConstraint ,
                          :Plan ,
                          :Task ,
                          :WorkFlow ;
         
         rdfs:comment "Description of an operational mission that can be used by the components performing it and that can be used for performence assessment."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#MissionModel

:MissionModel rdf:type owl:Class ;
              
              rdfs:subClassOf kern:Model .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#MissionObjective

:MissionObjective rdf:type owl:Class ;
                  
                  rdfs:subClassOf info:OperationalInformation ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty kern:isCodedIn ;
                                    owl:allValuesFrom info:String
                                  ] ;
                  
                  owl:disjointWith :MissionType ,
                                   :OperationalConstraint ,
                                   :Plan ,
                                   :Task ,
                                   :WorkFlow ;
                  
                  rdfs:comment "The objective of a mission can be described either has a high level task to perform or as a state to be reached."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#MissionStateMachine

:MissionStateMachine rdf:type owl:Class ;
                     
                     rdfs:subClassOf :FiniteStateMachine ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty kern:animates ;
                                       owl:someValuesFrom :MissionModel
                                     ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#MissionType

:MissionType rdf:type owl:Class ;
             
             rdfs:subClassOf info:OperationalInformation ;
             
             owl:disjointWith :OperationalConstraint ,
                              :Plan ,
                              :Task ,
                              :WorkFlow ;
             
             rdfs:comment "Textual or mathematical description of the kind of Mission for a specific Scenario."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#Negotiation

:Negotiation rdf:type owl:Class ;
             
             rdfs:subClassOf kern:InterAgentCommunication ;
             
             rdfs:comment "For solving conflicts or for assigning tasks agents have to communicate."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#OperationalConstraint

:OperationalConstraint rdf:type owl:Class ;
                       
                       rdfs:subClassOf info:Constraint ;
                       
                       owl:disjointWith :Plan ,
                                        :Task ,
                                        :WorkFlow ;
                       
                       rdfs:comment "This type of constraint defines requirements on the way the mission should be conducted and on minimal mission achievements."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#Operator

:Operator rdf:type owl:Class ;
          
          rdfs:subClassOf :Task ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDecomposition ;
                            owl:cardinality "0"^^xsd:nonNegativeInteger
                          ] ;
          
          rdfs:comment "An operator is an atomic task i.e. a task without decomposition."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#Plan

:Plan rdf:type owl:Class ;
      
      rdfs:subClassOf info:OperationalInformation ;
      
      owl:disjointWith :Task ,
                       :WorkFlow ;
      
      rdfs:comment "A plan has planned operators and is built by agents taking into account mission constraints and objective."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/mission.owl#PlanningSystem

:PlanningSystem rdf:type owl:Class ;
                
                rdfs:subClassOf <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticFunctionalSystem> ;
                
                owl:disjointWith :ReactiveSystem ,
