kern:TODO rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#aggregates

kern:aggregates rdfs:comment "indicates that systems will be aggregated in order to create a composite system"@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#produces

kern:produces rdfs:range kern:Information ;
              
              rdfs:domain kern:Probe .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#Partitions

:Partitions rdf:type owl:FunctionalProperty ,
                     owl:ObjectProperty ;
            
            rdfs:comment "This property states that the Time domain function partitions the set obtained thanks to the Time domain property when it is feasable."@en ;
            
            rdfs:range :PropertyTimeDomain .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#canBeAssociatedTo

:canBeAssociatedTo rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment "Some of the simulated objects with a correspondance to a physical object in the real world can be associated to a 3D representation in the simulation."@en ;
                   
                   rdfs:range info:Object3D ;
                   
                   rdfs:domain :SimulatedSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#canBeDeployedIn

:canBeDeployedIn rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "This property, sub property of Models, specifies that any system can be deployed in a simulator."@en ;
                 
                 rdfs:domain kern:System ;
                 
                 rdfs:subPropertyOf kern:isModelledBy ;
                 
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( :HardwareSimulator
                                            :HybridSimulator
                                            :SoftwareSimulator
                                          )
                            ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#creates

:creates rdf:type owl:ObjectProperty ;
         
         rdfs:comment "this property expresses the fact some object can create information"@en ;
         
         rdfs:range :Simulation ;
         
         rdfs:domain :SimulatorEvolutionModel .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasGlobalStateTransitionFunction

:hasGlobalStateTransitionFunction rdf:type owl:FunctionalProperty ,
                                           owl:ObjectProperty ;
                                  
                                  rdfs:subPropertyOf info:hasFunction ;
                                  
                                  rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasInput

:hasInput rdf:type owl:FunctionalProperty ,
                   owl:ObjectProperty ;
          
          rdfs:comment "Set used as source of the system specification"@en ;
          
          rdfs:range info:Set ;
          
          rdfs:subPropertyOf info:hasSet ;
          
          rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasInputSegments

:hasInputSegments rdf:type owl:FunctionalProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:comment "this property explicits the fact that there is a need to specify the time segments used over the input set. The input set is the set as defined by the property simulation:hasInputSet. The set of input segment is called OMEGA."^^xsd:string ;
                  
                  rdfs:domain :SystemSpecification ;
                  
                  rdfs:subPropertyOf :hasInput .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasInputSet

:hasInputSet rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:comment "The set of the different inputs"@en ;
             
             rdfs:range info:Set ;
             
             rdfs:domain :SystemSpecification ;
             
             rdfs:subPropertyOf :hasInput .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasModelReferenceSet

:hasModelReferenceSet rdf:type owl:FunctionalProperty ,
                               owl:ObjectProperty ;
                      
                      rdfs:subPropertyOf info:hasSet ;
                      
                      rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasOutput

:hasOutput rdf:type owl:FunctionalProperty ,
                    owl:ObjectProperty ;
           
           rdfs:comment "Set used as destination of the system specification"@en ;
           
           rdfs:range info:Set ;
           
           rdfs:subPropertyOf info:hasSet ;
           
           rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasOutputFunction

:hasOutputFunction rdf:type owl:FunctionalProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:subPropertyOf info:hasFunction ;
                   
                   rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasOutputSet

:hasOutputSet rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:comment "the set used as output"@en ;
              
              rdfs:range info:Set ;
              
              rdfs:domain :SystemSpecification ;
              
              rdfs:subPropertyOf :hasOutput .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasPointOfViewConfiguration

:hasPointOfViewConfiguration rdf:type owl:ObjectProperty ;
                             
                             rdfs:comment "null"@en ;
                             
                             rdfs:subPropertyOf kern:has ;
                             
                             rdfs:domain :SimulatedSceneCamera ;
                             
                             rdfs:range [ rdf:type owl:Class ;
                                          owl:unionOf ( info:Orientation3DEuler
                                                        info:Position3DCartesian
                                                        :Frustum
                                                      )
                                        ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasRelation

:hasRelation rdf:type owl:ObjectProperty ;
             
             rdfs:comment "A relation establishes a property that the elements of a set have to respect. This property allows to \"structure\" the set associated."@en ;
             
             rdfs:range info:Relation ;
             
             rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasSimulationOrientation

:hasSimulationOrientation rdf:type owl:ObjectProperty ;
                          
                          rdfs:range info:Orientation ;
                          
                          rdfs:subPropertyOf kern:has ;
                          
                          rdfs:domain :SimulatedPhysicalSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasSimulationPosition

:hasSimulationPosition rdf:type owl:ObjectProperty ;
                       
                       rdfs:range info:Position ;
                       
                       rdfs:subPropertyOf kern:has ;
                       
                       rdfs:domain :SimulatedPhysicalSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasStateSet

:hasStateSet rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:subPropertyOf info:hasSet ;
             
             rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasSystemSet

:hasSystemSet rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:subPropertyOf info:hasSet ;
              
              rdfs:range :SystemSet ;
              
              rdfs:domain :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasTimeBase

:hasTimeBase rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:comment "This property explains that any of the specification must be associated to at least one time base"^^xsd:string ;
             
             rdfs:subPropertyOf info:hasSet ;
             
             rdfs:domain :SystemSpecification ;
             
             rdfs:range :TimeBase .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasTimeDomainFunctionSet

:hasTimeDomainFunctionSet rdf:type owl:FunctionalProperty ,
                                   owl:ObjectProperty ;
                          
                          rdfs:range info:Set ;
                          
                          rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasTimeFactor

:hasTimeFactor rdf:type owl:ObjectProperty ;
               
               rdfs:comment "A render engine or a physics engine have a time factor."@en ;
               
               rdfs:range info:Real ;
               
               rdfs:subPropertyOf kern:has ;
               
               rdfs:domain :GraphicsEngine .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasTimeSample

:hasTimeSample rdf:type owl:ObjectProperty ;
               
               rdfs:comment "Time base has time samples."@en ;
               
               rdfs:range kern:Time ;
               
               rdfs:subPropertyOf kern:has ;
               
               rdfs:domain :TimeBase .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasTimeStep

:hasTimeStep rdf:type owl:ObjectProperty ;
             
             rdfs:comment "An render engine or a physics engine have a time step."@en ;
             
             rdfs:range info:Real ;
             
             rdfs:subPropertyOf kern:has ;
             
             rdfs:domain :GraphicsEngine .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasTrajectory

:hasTrajectory rdf:type owl:ObjectProperty ;
               
               rdfs:comment ""@en ;
               
               rdfs:subPropertyOf kern:has ;
               
               rdfs:domain :SimulatedPedestrian ;
               
               rdfs:range :Trajectory .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#hasValues

:hasValues rdf:type owl:FunctionalProperty ,
                    owl:ObjectProperty ;
           
           rdfs:comment "This property only indicates that a set is composed of values that are instances of the info:data"@en ;
           
           rdfs:domain info:Set ;
           
           rdfs:range kern:Data ;
           
           rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isAssociatedTo

:isAssociatedTo rdf:type owl:FunctionalProperty ,
                         owl:ObjectProperty ;
                
                rdfs:comment "This association states that a relation is between elements of the same set and thus associated to only one set."@en ;
                
                rdfs:range info:Set .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isComposedOfAlgorithmicModule

:isComposedOfAlgorithmicModule rdf:type owl:ObjectProperty ;
                               
                               rdfs:comment "this property allows an application to use module meaning also libraires and thus linking external libraries to application and to its inherited simulator"@en ;
                               
                               rdfs:domain kern:Application ;
                               
                               rdfs:range kern:SoftwareModule .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isComposedOfElements

:isComposedOfElements rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "This trajectory is composed of trajectory elements which represent the keyframes along the path."@en ;
                      
                      rdfs:range info:TrajectoryElement ;
                      
                      rdfs:domain :TrajectoryKeyFrames .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isCreatedBy

:isCreatedBy rdf:type owl:ObjectProperty ;
             
             rdfs:comment "This property allows to link Data to Probe"@en ;
             
             rdfs:domain :Simulation ;
             
             rdfs:range :SimulatorEvolutionModel ;
             
             owl:inverseOf :creates .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isPartitionedBy

:isPartitionedBy rdf:type owl:InverseFunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:comment "This property states that the set obtained through the Time domain property is partitionnedd by the time domain function."@en ;
                 
                 rdfs:range info:Function ;
                 
                 owl:inverseOf :Partitions .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isRepresentedBy

:isRepresentedBy rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "A 3D Object can be represented by several elements: meshes (one or several. e.g.: physical mesh and render mesh); texture mappings, object illumination ..."@en ;
                 
                 rdfs:domain info:Object3D ;
                 
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( :Illumination
                                            :Mesh
                                            :Textures
                                          )
                            ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#isSampleOf

:isSampleOf rdf:type owl:ObjectProperty ;
            
            rdfs:comment "Times are samples of time base."@en ;
            
            rdfs:domain kern:Time ;
            
            rdfs:subPropertyOf kern:isPropertyOf ;
            
            rdfs:range :TimeBase ;
            
            owl:inverseOf :hasTimeSample .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#mayCollaborateWith

:mayCollaborateWith rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "this property explains that a simulator can collaborate with another application. An application being a software, this application can also be a simulator."@en ;
                    
                    rdfs:range kern:Application .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#simulatesAnimationOf

:simulatesAnimationOf rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "this property is linked to animates property. It is the property allowing a simulator to fake animation of simulated systems."@en ;
                      
                      rdfs:range :SimulatedSystem ;
                      
                      rdfs:domain :SimulatorEvolutionModel .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#usesDiscreteTimeBase

:usesDiscreteTimeBase rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "The engines in a 3D virtual world simulation use  a discrete time base to achieve the execution of the simulation."@en ;
                      
                      rdfs:range :DiscreteTimeBase ;
                      
                      rdfs:domain :GraphicsEngine .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#usesRenderSceneCamera

:usesRenderSceneCamera rdf:type owl:ObjectProperty ;
                       
                       rdfs:comment "The render engine uses and manages the render camera of the scene. It controls the point of view of the 3D world."@en ;
                       
                       rdfs:domain :RenderEngine ;
                       
                       rdfs:range :SimulatedSceneCamera .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#withDeployementBecomes

:withDeployementBecomes rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment "expresses the fact that a Simulated system models specifically a system"@en ;
                        
                        rdfs:domain kern:System ;
                        
                        rdfs:subPropertyOf kern:isModelledBy ;
                        
                        rdfs:range :SimulatedSystem .





#################################################################
#
#    Data properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#Configuration

:Configuration rdf:type owl:DatatypeProperty ;
               
               rdfs:comment "Configuration means to complete the simulator information in order to allow it to provide life to a system. At this step the configuration does not mean the configuration of the system to be simulated but information on the simulator itself, its distribution properties onto a hardware architecture, ..."@en ;
               
               rdfs:domain :SimulatorEvolutionModel .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#EnvironmentMeshes

:EnvironmentMeshes rdf:type owl:DatatypeProperty ;
                   
                   rdfs:comment "PhysicsEngines use Meshes in order to represent many parts of the environment. This Data type is inherited of what can be found in the Data tree of the information ontology"@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#dimension

:dimension rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           
           rdfs:comment "number of set in the cross product"@en ;
           
           rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#homogeneous

:homogeneous rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:comment "this homogeenous property indicates that sets composing the cross product are identical (useful to create Rn and In). False means it is not homogeneous."@en ;
             
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "false"^^xsd:boolean ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "true"^^xsd:boolean ;
                                                 rdf:rest rdf:nil
                                               ]
                                    ]
                        ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#manageDifferentialEquationAlgorithmicSoftwareModules

:manageDifferentialEquationAlgorithmicSoftwareModules rdf:type owl:DatatypeProperty ;
                                                      
                                                      rdfs:comment "This property indicates that the software simulator is able to manage software modules  that are using the differential equation theory."@en ;
                                                      
                                                      rdfs:domain :SoftwareSimulator ;
                                                      
                                                      rdfs:range [ rdf:type rdfs:Datatype ;
                                                                   owl:oneOf [ rdf:type rdf:List ;
                                                                               rdf:first "false"^^xsd:boolean ;
                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                          rdf:first "true"^^xsd:boolean ;
                                                                                          rdf:rest rdf:nil
                                                                                        ]
                                                                             ]
                                                                 ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#managePartialDerivativeEquationAlgorithmicSoftwareModules

:managePartialDerivativeEquationAlgorithmicSoftwareModules rdf:type owl:DatatypeProperty ;
                                                           
                                                           rdfs:comment "this paramater indicates the capability of the software simulator to manipulate software Modules using the Derivative equation theory"@en ;
                                                           
                                                           rdfs:domain :SoftwareSimulator ;
                                                           
                                                           rdfs:range [ rdf:type rdfs:Datatype ;
                                                                        owl:oneOf [ rdf:type rdf:List ;
                                                                                    rdf:first "false"^^xsd:boolean ;
                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "true"^^xsd:boolean ;
                                                                                               rdf:rest rdf:nil
                                                                                             ]
                                                                                  ]
                                                                      ] .





