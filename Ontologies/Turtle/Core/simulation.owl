###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#AssociativeArrayForSimulation

info:AssociativeArrayForSimulation rdf:type owl:Class .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#AudioPacket

info:AudioPacket owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#CANFrame

info:CANFrame owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Constraint

info:Constraint owl:disjointWith :PropertyTimeDomain ,
                                 :Trajectory .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#CrossProduct

info:CrossProduct owl:disjointWith :Frustum ,
                                   :SystemSet ,
                                   :TimeBase .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#GPS

info:GPS owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#I

info:I owl:disjointWith :Frustum ,
                        :SystemSet ,
                        :TimeBase .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Image

info:Image owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#LaserScan

info:LaserScan owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Message

info:Message owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#MissionEvaluation

info:MissionEvaluation owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Object2D

info:Object2D owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Object3D

info:Object3D owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#OrderedSet

info:OrderedSet owl:disjointWith :SystemSpecification .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Orientation2DAngle

info:Orientation2DAngle owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Orientation2DMatrix

info:Orientation2DMatrix owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Orientation3DEuler

info:Orientation3DEuler owl:disjointWith :Simulation .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Orientation3DMatrix

info:Orientation3DMatrix owl:disjointWith :Simulation .



