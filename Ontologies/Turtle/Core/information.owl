                                                                                                   owl:imports <http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl> ,
                                                                                                               <http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl> ,
                                                                                                               <http://swrl.stanford.edu/ontologies/3.3/swrla.owl> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#TODO

kern:TODO rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2000/01/rdf-schema#comment

rdfs:comment rdf:type owl:AnnotationProperty .





#################################################################
#
#    Datatypes
#
#################################################################


###  http://www.w3.org/2001/XMLSchema#anySimpleType

xsd:anySimpleType rdf:type rdfs:Datatype .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#canBeSavedin

:canBeSavedin rdf:type owl:ObjectProperty ;
              
              rdfs:comment "Any kind of Data can be saved or stored in a file of the system."@en ;
              
              rdfs:range :ComputerFile ;
              
              rdfs:domain kern:Data .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#contains

:contains rdf:type owl:ObjectProperty ;
          
          rdfs:comment "Relation between a group and its elements"@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#containsAbstraction

:containsAbstraction rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "This property is the mirror of the info:containsData property linking Collection to its values. It links Set to its values in Abstraction."@en ;
                     
                     rdfs:domain :Set ;
                     
                     rdfs:subPropertyOf :contains ;
                     
                     rdfs:range kern:Abstraction .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#containsAsKeys

:containsAsKeys rdf:type owl:ObjectProperty ;
                
                rdfs:comment "To specify the keys used for each data in this AssociativeArray."^^xsd:string ;
                
                rdfs:domain :AssociativeArray ;
                
                rdfs:subPropertyOf :containsData ;
                
                rdfs:range kern:Data .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#containsData

:containsData rdf:type owl:ObjectProperty ;
              
              rdfs:comment "This property is the mirror of the info:containsAbstraction property linking Set to its values. It links Collection to its values in Data."@en ;
              
              rdfs:domain :Collection ;
              
              rdfs:subPropertyOf :contains ;
              
              rdfs:range kern:Data .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#has3DRepresentation

:has3DRepresentation rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "A physical object can be represented by a 3D object in a virtual world."@en ;
                     
                     rdfs:range :Object3D ;
                     
                     rdfs:domain kern:PhysicalObject ;
                     
                     rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasAMathematicalDatum

:hasAMathematicalDatum rdf:type owl:ObjectProperty ;
                       
                       rdfs:domain :PhysicalData ;
                       
                       rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasAPhysicalQuantity

:hasAPhysicalQuantity rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "The physical quantity of a given unit"^^xsd:string ;
                      
                      rdfs:range :PhysicalQuantity ;
                      
                      rdfs:subPropertyOf kern:has ;
                      
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :PhysicalData
                                                  :Unit
                                                )
                                  ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasAUnit

:hasAUnit rdf:type owl:ObjectProperty ;
          
          rdfs:domain :PhysicalData ;
          
          rdfs:range :Unit ;
          
          rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasFunction

:hasFunction rdf:type owl:ObjectProperty ;
             
             rdfs:range :Function ;
             
             rdfs:subPropertyOf kern:has ;
             
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :OrderedSet
                                         :StructuredData
                                       )
                         ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasInput

:hasInput rdf:type owl:FunctionalProperty ,
                   owl:ObjectProperty ;
          
          rdfs:comment "specific properties of Functions that associate Set to Set"@en ;
          
          rdfs:domain :Function ;
          
          rdfs:range :Set ;
          
          rdfs:subPropertyOf :hasSet .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasOutput

:hasOutput rdf:type owl:FunctionalProperty ,
                    owl:ObjectProperty ;
           
           rdfs:comment "property associated to Function that links Set to Set"@en ;
           
           rdfs:domain :Function ;
           
           rdfs:range :Set ;
           
           rdfs:subPropertyOf :hasSet .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasPath

:hasPath rdf:type owl:ObjectProperty ;
         
         rdfs:comment "A file has a string descriptor for its path which indicates its localization in the file system of the Operating System."@en ;
         
         rdfs:domain :ComputerFile ;
         
         rdfs:range :String ;
         
         rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasPhysicalCharacteristics

:hasPhysicalCharacteristics rdf:type owl:ObjectProperty ;
                            
                            rdfs:comment "A physical object can have phycal information like, length, position, velocity, acceleration, etc.."@en ;
                            
                            rdfs:range :PhysicalData ;
                            
                            rdfs:domain kern:PhysicalObject ;
                            
                            rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasPositionRepresentation

:hasPositionRepresentation rdf:type owl:ObjectProperty ;
                           
                           rdfs:comment "A position can have different types of representation depending on the world reference."@en ;
                           
                           rdfs:domain :Position ;
                           
                           rdfs:subPropertyOf kern:has ;
                           
                           rdfs:range [ rdf:type owl:Class ;
                                        owl:unionOf ( :Position2DCartesian
                                                      :Position2DPolar
                                                      :Position3DCartesian
                                                      :Position3DCylindrical
                                                      :Position3DSpherical
                                                      :Position6D
                                                    )
                                      ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasSet

:hasSet rdf:type owl:ObjectProperty ;
        
        rdfs:comment "Whatever the information it can asks to be associated to Set."@en ;
        
        rdfs:range :Set ;
        
        rdfs:domain kern:Information ;
        
        rdfs:subPropertyOf kern:has .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#isRepresented

:isRepresented rdf:type owl:ObjectProperty ;
               
               rdfs:comment "A file is represented by a binary stream stored in the file system of the operating system."@en ;
               
               rdfs:range :Binary ;
               
               rdfs:domain :ComputerFile .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#managesFile

:managesFile rdf:type owl:ObjectProperty ;
             
             rdfs:comment "A computer file is indexed and stored in an File System. It represents the standard storage unit of the system."@en ;
             
             rdfs:range :ComputerFile ;
             
             rdfs:domain kern:FileSystem .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasInput

<http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#hasInput> rdf:type owl:ObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Dimension

:Dimension rdf:type owl:DatatypeProperty ;
           
           rdfs:comment "Number of set of the cross product"@en ;
           
           rdfs:domain :CrossProduct ;
           
           rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_UTC_time

:gps_UTC_time rdf:type owl:DatatypeProperty ;
              
              rdfs:comment "UTC time for the current position"^^xsd:string ;
              
              rdfs:domain :GPS ;
              
              rdfs:range xsd:dateTime .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_altitude_1

:gps_altitude_1 rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:comment "Altitude above mean sea level (in meters)"^^xsd:string ;
                
                rdfs:domain :GPS ;
                
                rdfs:range xsd:double .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_altitude_2

:gps_altitude_2 rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:comment "Altitude above ellipsoid (in meters)"^^xsd:string ;
                
                rdfs:domain :GPS ;
                
                rdfs:range xsd:double .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_heading

:gps_heading rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:comment "Current heading in degrees"^^xsd:string ;
             
             rdfs:domain :GPS ;
             
             rdfs:range xsd:double .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_latitude

:gps_latitude rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:comment "WGS84 latitude (in degrees)"^^xsd:string ;
              
              rdfs:domain :GPS ;
              
              rdfs:range xsd:double .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_longitude

:gps_longitude rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               
               rdfs:comment "WGS84 longitude (in degrees)"^^xsd:string ;
               
               rdfs:domain :GPS ;
               
               rdfs:range xsd:double .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_nb_sats

:gps_nb_sats rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:comment "Number of satellites in view"^^xsd:string ;
             
             rdfs:domain :GPS ;
             
             rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_quality_indicator

:gps_quality_indicator rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       
                       rdfs:comment """Normalized quality indicator.
0 = invalid
1 = GPS fix (SPS)
2 = DGPS fix
3 = PPS fix
4 = Real Time Kinematic
5 = Float RTK
6 = estimated (dead reckoning) (2.3 feature)
7 = Manual input mode
8 = Simulation mode"""^^xsd:string ;
                       
                       rdfs:domain :GPS ;
                       
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:oneOf [ rdf:type rdf:List ;
                                                rdf:first "0"^^xsd:int ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "1"^^xsd:int ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "2"^^xsd:int ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "3"^^xsd:int ;
                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                            rdf:first "4"^^xsd:int ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first "5"^^xsd:int ;
                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                  rdf:first "6"^^xsd:int ;
                                                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                                                             rdf:first "7"^^xsd:int ;
                                                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                                                        rdf:first "8"^^xsd:int ;
                                                                                                                                        rdf:rest rdf:nil
                                                                                                                                      ]
                                                                                                                           ]
                                                                                                                ]
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                  ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_speed_east

:gps_speed_east rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:comment "Current speed - East component (in m/s)"^^xsd:string ;
                
                rdfs:domain :GPS ;
                
                rdfs:range xsd:float .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_speed_north

:gps_speed_north rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 
                 rdfs:comment "Current speed - North component (in m/s)"^^xsd:string ;
                 
                 rdfs:domain :GPS ;
                 
                 rdfs:range xsd:float .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#gps_speed_up

:gps_speed_up rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:comment "Current speed - Up component (in m/s)"^^xsd:string ;
              
              rdfs:domain :GPS ;
              
              rdfs:range xsd:float .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#hasValue

:hasValue rdf:type owl:DatatypeProperty ;
          
          rdfs:comment "Protégé primitive value for Primitive data"^^xsd:string ;
          
          rdfs:domain :PrimitiveData ;
          
          rdfs:subPropertyOf owl:topDataProperty .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#homogeneous

:homogeneous rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:comment "this property indicates the fact that the sets of the cross product are identical or not"@en ;
             
             rdfs:domain :CrossProduct ;
             
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "false"^^xsd:boolean ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "true"^^xsd:boolean ;
                                                 rdf:rest rdf:nil
                                               ]
                                    ]
                        ] .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_coding

:image_coding rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:comment "Usually a four characters code"^^xsd:string ;
              
              rdfs:domain :Image ;
              
              rdfs:range xsd:string .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_data

:image_data rdf:type owl:DatatypeProperty ;
            
            rdfs:comment "Pixel data bytes"^^xsd:string ;
            
            rdfs:domain :Image ;
            
            rdfs:range xsd:byte .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_depth

:image_depth rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:comment "Depth of current pixels (usually 8 bits but can also be 10, 12, 16...)"^^xsd:string ;
             
             rdfs:domain :Image ;
             
             rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_height

:image_height rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:comment "Number of lines."^^xsd:string ;
              
              rdfs:domain :Image ;
              
              rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_nb_channels

:image_nb_channels rdf:type owl:DatatypeProperty ;
                   
                   rdfs:comment "Number of channels in image (ex : GRAY = 1 chan, RGB  = 3 chans, RGBA = 4 chans)"^^xsd:string ;
                   
                   rdfs:domain :Image ;
                   
                   rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_size

:image_size rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            
            rdfs:comment "Size of pixel data in bytes."^^xsd:string ;
            
            rdfs:domain :Image ;
            
            rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#image_width

:image_width rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:comment "Number of pixels per line."^^xsd:string ;
             
             rdfs:domain :Image ;
             
             rdfs:range xsd:int .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#isTimestampedBy

:isTimestampedBy rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 
                 rdfs:comment "this property memorises a date and hour associated to the Data itself. There is here neither description of the source of this tag (creation of the Data, emission of the data, else) nor of the entity responsible for the tag (physical or else)."@en ;
                 
                 rdfs:domain :TimeStampedData ;
                 
                 rdfs:range xsd:dateTime ;
                 
                 rdfs:subPropertyOf owl:topDataProperty .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#producedTimestamp

:producedTimestamp rdf:type owl:DatatypeProperty ;
                   
                   rdfs:domain kern:Clock ;
                   
                   rdfs:range xsd:dateTime .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#x

:x rdf:type owl:DatatypeProperty ,
            owl:FunctionalProperty ;
   
   rdfs:domain :Position2DCartesian ;
   
   rdfs:range xsd:double .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#y

:y rdf:type owl:DatatypeProperty ,
            owl:FunctionalProperty ;
   
   rdfs:domain :Position2DCartesian ;
   
   rdfs:range xsd:double .



###  http://www.w3.org/2002/07/owl#topDataProperty

owl:topDataProperty rdf:type owl:DatatypeProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#ASCIICharacter

:ASCIICharacter rdf:type owl:Class ;
                
                rdfs:subClassOf :Character ;
                
                owl:disjointWith :UTF8Character .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Acceleration

:Acceleration rdf:type owl:Class ;
              
              rdfs:subClassOf :PhysicalData ;
              
              owl:disjointWith :Angle ,
                               :Date ,
                               :Distance ,
                               :Duration ,
                               :ElectricField ,
                               :ElectricalCurrent ,
                               :Energy ,
                               :Force ,
                               :Humidity ,
                               :InstantPower ,
                               :Luminosity ,
                               :MagneticField ,
                               :Mass ,
                               :Orientation ,
                               :Position ,
                               :Pressure ,
                               :RotationRate ,
                               :Speed ,
                               :Temperature ,
                               :Torque ,
                               :Voltage ,
                               :Wavelength ,
                               :Weight .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#Angle

:Angle rdf:type owl:Class ;
       
       rdfs:subClassOf :PhysicalData ;
       
       owl:disjointWith :Date ,
                        :Distance ,
                        :Duration ,
                        :ElectricField ,
                        :ElectricalCurrent ,
                        :Energy ,
                        :Force ,
                        :Humidity ,
                        :InstantPower ,
                        :Luminosity ,
                        :MagneticField ,
                        :Mass ,
                        :Orientation ,
                        :Position ,
                        :Pressure ,
                        :RotationRate ,
                        :Speed ,
                        :Temperature ,
                        :Torque ,
                        :Voltage ,
                        :Wavelength ,
                        :Weight .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#AssociativeArray

:AssociativeArray rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Collection ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :containsAsKeys ;
                                    owl:someValuesFrom kern:Data
                                  ] ;
                  
                  owl:disjointWith :Sequence ;
                  
                  rdfs:comment "A set of pairs key/value where keys are unique."^^xsd:string .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#AssociativeArrayForSimulation

:AssociativeArrayForSimulation rdf:type owl:Class ;
                               
                               rdfs:subClassOf :AssociativeArray ;
                               
                               owl:disjointWith :I ,
                                                :OrderedSetForSystemDescription ,
                                                :R ;
                               
                               rdfs:comment "Any collection used for simulation purpose. As an Application, a simulation consumes lots of data such meshes of configuration files."@en .



###  http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#AudioPacket

:AudioPacket rdf:type owl:Class ;
             
             rdfs:subClassOf :ComposedData ;
             
             owl:disjointWith :CANFrame ,
                              :GPS ,
                              :Image ,
                              :LaserScan ,
                              :Message ,
                              :MissionEvaluation ,
                              :Object2D ,
