:CrossProduct rdf:type owl:Class ;
              
              rdfs:subClassOf :Set ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :containsAbstraction ;
                                owl:allValuesFrom :Set
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :Dimension ;
                                owl:minCardinality "2"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
