    <rdfs:domain rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Probe"/>
  </rdf:Description>
  <simu:SoftwareSimulator rdf:ID="SimulatorForDifferentialEquationsAndExternalModels">
    <rdfs:comment xml:lang="en">This instance should allow us to verify that our simulator representation is workinng</rdfs:comment>
    <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</kern:isAtomic>
  </simu:SoftwareSimulator>
