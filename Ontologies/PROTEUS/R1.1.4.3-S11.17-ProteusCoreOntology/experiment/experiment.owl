                </owl:disjointWith>
                <owl:disjointWith rdf:resource="#Problem"/>
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Robot.owl#Robot"/>
                <owl:disjointWith>
                  <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Robot.owl#GlobalPlatform">
