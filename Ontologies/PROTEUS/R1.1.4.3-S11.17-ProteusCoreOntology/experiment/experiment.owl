<?xml version="1.0"?>
<rdf:RDF
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
    xmlns:robo="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Robot.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:expe="http://www.anr-proteus.fr/sites/default/files/download/Ontology/experiment.owl#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
    xmlns:info="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#"
  xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/experiment.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl"/>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl"/>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl"/>
    <rdfs:comment xml:lang="en">This work has been funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project. It remains the property of the partners of the PROTEUS consortium but is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
    <rdfs:comment xml:lang="en">This ontology describes the lifecycle process adopted in the PROTEUS project for the problem builders and solution providers</rdfs:comment>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl"/>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Robot.owl"/>
    <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >version delivered 1.0</owl:versionInfo>
    <owl:imports rdf:resource="http://swrl.stanford.edu/ontologies/3.3/swrla.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="SimulatedEnvironment">
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedPhysicalSystem"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#models"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="EnvironmentModel"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#models"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedVehicle">
        <owl:disjointWith rdf:resource="#SimulatedEnvironment"/>
      </rdf:Description>
    </owl:disjointWith>
    <owl:disjointWith>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedPedestrian">
        <owl:disjointWith rdf:resource="#SimulatedEnvironment"/>
      </rdf:Description>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">A simulated environment implements a modelled environment</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ChallengeProvider">
    <rdfs:comment xml:lang="en">A Challenge Provider is first a problem provider but he / she also provides the environment required for a competition to occur as for example real robots, arena, and so on and so forth.</rdfs:comment>
    <owl:disjointWith>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Pedestrian">
        <owl:disjointWith>
          <owl:Class rdf:ID="Challenger"/>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="#ChallengeProvider"/>
      </rdf:Description>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Challenger"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ProblemProvider"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Configuration">
    <owl:disjointWith>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#AutomataState">
        <owl:disjointWith rdf:resource="#Configuration"/>
      </rdf:Description>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#State">
        <owl:disjointWith>
          <owl:Class rdf:ID="Scenario"/>
        </owl:disjointWith>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Problem"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#isUsedBy"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A configuration is a particular Initial state of a System</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Challenger">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The challenger is a solution provider that accepts to participate to a competion, a challenge as organised by a ChallengeProvider.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Pedestrian"/>
    <owl:disjointWith rdf:resource="#ChallengeProvider"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SolutionProvider"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EnvironmentSimulator">
    <rdfs:comment xml:lang="en">This simulator goal is to implement a model of the environment into an application. Nevrtheless it models the environment.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TODO: even if kern:models some kern:Environment, question remains to see if it is not more a expe:Environment Model</owl:versionInfo>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SoftwareSimulator">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:allValuesFrom>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#OperatingSystem">
                <owl:disjointWith>
                  <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedSystem">
                    <owl:disjointWith>
                      <owl:Class rdf:ID="Solution"/>
                    </owl:disjointWith>
                    <owl:disjointWith>
                      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Application">
                        <rdfs:subClassOf>
                          <owl:Restriction>
                            <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#executesOn"/>
                            <owl:allValuesFrom rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#Computer"/>
                          </owl:Restriction>
                        </rdfs:subClassOf>
                        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedSystem"/>
                        <owl:disjointWith>
                          <owl:Class rdf:about="#Solution"/>
                        </owl:disjointWith>
                      </rdf:Description>
                    </owl:disjointWith>
                    <owl:disjointWith>
                      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#FileSystem">
                        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedSystem"/>
                        <owl:disjointWith>
                          <owl:Class rdf:about="#Solution"/>
                        </owl:disjointWith>
                      </rdf:Description>
                    </owl:disjointWith>
                    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#OperatingSystem"/>
                    <owl:disjointWith>
                      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#SystemInterface">
                        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedSystem"/>
                        <owl:disjointWith>
                          <owl:Class rdf:about="#Solution"/>
                        </owl:disjointWith>
                      </rdf:Description>
                    </owl:disjointWith>
                  </rdf:Description>
                </owl:disjointWith>
                <owl:disjointWith>
                  <owl:Class rdf:about="#Solution"/>
                </owl:disjointWith>
              </rdf:Description>
            </owl:allValuesFrom>
            <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#isAnimatedByEvolutionModel"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith>
          <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine">
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#SoftwareProbe">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Wrapper">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Driver">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#AlgorithmicModule">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#RoboticApplication">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SoftwareSimulator"/>
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#RoboticMiddleware">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SoftwareSimulator"/>
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#GraphicsEngine"/>
              </rdf:Description>
            </owl:disjointWith>
          </rdf:Description>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#RoboticMiddleware"/>
        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#RoboticApplication"/>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#models"/>
        <owl:allValuesFrom>
          <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment">
            <owl:disjointWith>
              <owl:Class rdf:about="#Problem"/>
            </owl:disjointWith>
          </rdf:Description>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="GenerateSimulator">
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Action">
        <owl:disjointWith>
          <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#NoiseSignal">
            <owl:disjointWith>
              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#ApplicationInteraction">
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#NoiseSignal"/>
                <owl:disjointWith>
                  <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#InterAgentCommunication">
                    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#NoiseSignal"/>
                    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#ApplicationInteraction"/>
                    <owl:disjointWith>
                      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#HardwareToDriverInteraction">
                        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#NoiseSignal"/>
                        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#InterAgentCommunication"/>
                        <owl:disjointWith>
                          <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Force">
                            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#NoiseSignal"/>
                            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#ApplicationInteraction"/>
                            <owl:disjointWith>
                              <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#DriverToHardwareInteraction">
                                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#NoiseSignal"/>
                                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#InterAgentCommunication"/>
                                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Force"/>
                              </rdf:Description>
                            </owl:disjointWith>
                            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#HardwareToDriverInteraction"/>
                            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#InterAgentCommunication"/>
                            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Action"/>
                          </rdf:Description>
                        </owl:disjointWith>
                      </rdf:Description>
                    </owl:disjointWith>
                    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#DriverToHardwareInteraction"/>
                    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Action"/>
                    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Force"/>
                  </rdf:Description>
                </owl:disjointWith>
                <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Force"/>
              </rdf:Description>
            </owl:disjointWith>
            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#DriverToHardwareInteraction"/>
            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#HardwareToDriverInteraction"/>
            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#InterAgentCommunication"/>
            <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Force"/>
          </rdf:Description>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#ApplicationInteraction"/>
        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#InterAgentCommunication"/>
        <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#Force"/>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">a user of the robot modelling platform is creating a simulator through this action using the RobotML platform.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Solution">
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Application"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#OperatingSystem"/>
    <rdfs:comment xml:lang="en">A Solution is an answer to a Problem. It is provided by a Challenger. It is the implementation of the System targeted by the problem. The solution is a part of the Software of the Robot system. thus a part of an RoboticApplication.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#CompositeSystem"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TODO: for the time being there is no existing relation to the Problem thus there should be a link created.</owl:versionInfo>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#FileSystem"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#AlgorithmicModule"/>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#aggregates"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#RoboticApplication"/>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#aggregates"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#SimulatedSystem"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#SystemInterface"/>
  </owl:Class>
  <owl:Class rdf:ID="RobotMLPlatformUser">
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Human">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#canBeDeployedIn"/>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:cardinality>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#withDeployementBecomes"/>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:cardinality>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#isModelledBy"/>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:cardinality>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:cardinality>
            <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#models"/>
          </owl:Restriction>
        </rdfs:subClassOf>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="ModellingActivity"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#hasActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">This user has the capability to model and generate simulator using the Robot ML platform.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#SolutionProvider">
    <rdfs:subClassOf rdf:resource="#RobotMLPlatformUser"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Solution"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="Creates"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#Creates"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
