<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
    xmlns:envi="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:kern="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
  xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment xml:lang="en">This work has been funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project. It remains the property of the partners of the PROTEUS consortium but is governed by the CECILL B V1  licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >version delivered 1.0</owl:versionInfo>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl"/>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl"/>
    <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Atmosphere">
    <owl:disjointWith>
      <owl:Class rdf:ID="Waterbody"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">a layer of gases that may surround a planet ]and that is held in place by the gravity of the planet. It is of interest if there are influences exercising on included physical objects.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FluidicBody"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SolidBody">
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#FluidicBody"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="AerodynamicForce">
    <rdfs:comment xml:lang="en">Aerodynamic force is the resultant force exerted on a body by the air (or some other gas) in which the body is immersed, and is due to the relative motion between the body and the fluid.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="NormalForce"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="GravityForce"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Buoyancy"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Force"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#systemSource"/>
        <owl:allValuesFrom rdf:resource="#Atmosphere"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Force">
    <rdfs:comment xml:lang="en">A force is any influence that causes a free body to undergo a change in speed, a change in direction, or a change in shape. It is an interaction as it can be caused either by another body or as a synthesis of the environment influence (the two of them are systems).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Interaction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#systemSource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
        <owl:onProperty rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#impacts"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Pedestrian">
    <rdfs:comment xml:lang="en">Physical Agent in a scenario which perfoms the action of going on foot, i.e. walking.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Human"/>
  </owl:Class>
  <owl:Class rdf:ID="Stairs">
    <rdfs:comment xml:lang="en">a construction designed to bridge a large vertical distance by dividing it into smaller vertical distances, called steps.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Floor"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="LandSurface"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="WaterSurface"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Surface"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class>
    <owl:unionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment">
        <owl:disjointWith>
          <owl:Class rdf:ID="Planet"/>
        </owl:disjointWith>
        <owl:disjointWith>
          <owl:Class rdf:about="#Surface"/>
        </owl:disjointWith>
        <owl:disjointWith>
          <owl:Class rdf:ID="Building"/>
        </owl:disjointWith>
        <owl:disjointWith>
          <owl:Class rdf:about="#FluidicBody"/>
        </owl:disjointWith>
      </rdf:Description>
    </owl:unionOf>
  </owl:Class>
  <owl:Class rdf:about="#Waterbody">
    <rdfs:comment xml:lang="en">Significant accumulation of water. It is of interest if there are influences exercising on included physical objects.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Atmosphere"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#FluidicBody"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#NormalForce">
    <rdfs:comment xml:lang="en">The normal force is the repulsive force of interaction between atoms at close contact. This force is applicalble in hard surface contact of physical objects.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Force"/>
    <owl:disjointWith rdf:resource="#AerodynamicForce"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#GravityForce"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Buoyancy"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#WaterSurface">
    <rdfs:comment xml:lang="en">Surface of an ocean, a sea, a lake or an artificial reservatory.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Stairs"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Floor"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#LandSurface"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isSurfaceOf"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#Waterbody"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Surface"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Floor">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Surface"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Walking surface of a room in a building.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Stairs"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#LandSurface"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#WaterSurface"/>
  </owl:Class>
  <owl:Class rdf:about="#Building">
    <owl:disjointWith>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Agent">
        <owl:disjointWith>
          <owl:Class rdf:about="#Planet"/>
        </owl:disjointWith>
        <owl:disjointWith>
          <owl:Class rdf:about="#Surface"/>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="#Building"/>
        <owl:disjointWith>
          <owl:Class rdf:about="#FluidicBody"/>
        </owl:disjointWith>
      </rdf:Description>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment"/>
    <owl:disjointWith>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware">
        <owl:disjointWith>
          <owl:Class rdf:about="#Planet"/>
        </owl:disjointWith>
        <owl:disjointWith>
          <owl:Class rdf:about="#Surface"/>
        </owl:disjointWith>
        <owl:disjointWith rdf:resource="#Building"/>
        <owl:disjointWith>
          <owl:Class rdf:about="#FluidicBody"/>
        </owl:disjointWith>
      </rdf:Description>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Planet"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Surface"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#FluidicBody"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">human-made structure used or intended for supporting or sheltering any use or continuous occupancy</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#SolidBody"/>
  </owl:Class>
  <owl:Class rdf:about="#Planet">
    <rdfs:comment xml:lang="en">a celestial body  that is massive enough to be rounded by its own gravity</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Agent"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Surface"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Building"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#FluidicBody"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#LandSurface">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isSurfaceOf"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Planet"/>
              <owl:Class rdf:about="#SolidBody"/>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Surface"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Stairs"/>
    <owl:disjointWith rdf:resource="#Floor"/>
    <owl:disjointWith rdf:resource="#WaterSurface"/>
    <rdfs:comment xml:lang="en">The solid surface of a planet.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#GravityForce">
    <rdfs:subClassOf rdf:resource="#Force"/>
    <rdfs:comment xml:lang="en">A force that is directly proportional to the PhysicalObject mass.</rdfs:comment>
    <owl:disjointWith rdf:resource="#AerodynamicForce"/>
    <owl:disjointWith rdf:resource="#NormalForce"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Buoyancy"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#FluidicBody">
    <owl:disjointWith rdf:resource="#Planet"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment"/>
    <owl:disjointWith rdf:resource="#SolidBody"/>
    <owl:disjointWith rdf:resource="#Building"/>
    <rdfs:comment xml:lang="en">Fluidic bodies are physical objects that have current in their fluids.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Agent"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware"/>
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Surface"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Buoyancy">
    <owl:disjointWith rdf:resource="#AerodynamicForce"/>
    <owl:disjointWith rdf:resource="#NormalForce"/>
    <owl:disjointWith rdf:resource="#GravityForce"/>
    <rdfs:comment xml:lang="en">Buoyancy is an upward acting force exerted by a fluid, that opposes an object's weight. This force is relevant for physical objects in waterbodies or at watersurface.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Force"/>
  </owl:Class>
  <owl:Class rdf:about="#Surface">
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware"/>
    <rdfs:comment xml:lang="en">A two-dimensional topological manifold that is a boundary of a solid object.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
    <owl:disjointWith rdf:resource="#Planet"/>
    <owl:disjointWith rdf:resource="#FluidicBody"/>
    <owl:disjointWith rdf:resource="#Building"/>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Agent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasSurface"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >0</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasAtmosphere">
    <rdfs:range rdf:resource="#Atmosphere"/>
    <rdfs:comment xml:lang="en">Some planets have atmosphere</rdfs:comment>
    <rdfs:domain rdf:resource="#Planet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isLocatedOver">
    <rdfs:range rdf:resource="#LandSurface"/>
    <rdfs:comment xml:lang="en">Buildings are located over land surface.</rdfs:comment>
    <rdfs:domain rdf:resource="#Building"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCurrentInFluid">
    <rdfs:domain rdf:resource="#FluidicBody"/>
    <rdfs:comment xml:lang="en">Atmospheres and waterbodies may have distribution of current in them and it is one of their physical characteristics</rdfs:comment>
    <rdfs:range rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#Tensor"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasStairs">
    <rdfs:comment xml:lang="en">Stairs are usually in buildings, but some stairs are outdoor.</rdfs:comment>
    <rdfs:range rdf:resource="#Stairs"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Building"/>
          <owl:Class rdf:about="#Planet"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasLandSurface">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hasSurface"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#Planet"/>
    <rdfs:comment xml:lang="en">A planet has a land surface whose vertical and horizontal dimensions characteirse the terrain.</rdfs:comment>
    <rdfs:range rdf:resource="#LandSurface"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasBuilding">
    <rdfs:range rdf:resource="#Building"/>
    <rdfs:comment xml:lang="en">Planets such as Earth have buildings.</rdfs:comment>
    <rdfs:domain rdf:resource="#Planet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasSurface">
    <rdfs:domain rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
    <rdfs:comment xml:lang="en">Physical objects have surfaces that determine their boudaries.</rdfs:comment>
    <rdfs:range rdf:resource="#Surface"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#isSurfaceOf"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isSurfaceOf">
    <owl:inverseOf rdf:resource="#hasSurface"/>
    <rdfs:range rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
    <rdfs:domain rdf:resource="#Surface"/>
    <rdfs:comment xml:lang="en">Surfaces are the borders of physical objects.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasWaterbody">
    <rdfs:range rdf:resource="#Waterbody"/>
    <rdfs:domain rdf:resource="#Planet"/>
    <rdfs:comment xml:lang="en">Planets have waterbodies such as oceans, seas...</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasFloor">
    <rdfs:comment xml:lang="en">buildings have floors</rdfs:comment>
    <rdfs:domain rdf:resource="#Building"/>
    <rdfs:range rdf:resource="#Floor"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="accelerates">
    <rdfs:comment xml:lang="en">force accelerates physical object</rdfs:comment>
    <rdfs:domain rdf:resource="#Force"/>
    <rdfs:range rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#PhysicalObject"/>
  </owl:ObjectProperty>
  <rdf:Property rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#hasPhysicalCharacteristic"/>
</rdf:RDF>

