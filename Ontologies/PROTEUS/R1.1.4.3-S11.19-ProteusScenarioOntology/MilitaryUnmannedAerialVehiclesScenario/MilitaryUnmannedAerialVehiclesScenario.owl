<?xml version="1.0"?>
<rdf:RDF
    xmlns:syst="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#"
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
    xmlns:robo="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Robot.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:mili="http://www.anr-proteus.fr/sites/default/files/download/Ontology/MilitaryUnmannedAerialVehiclesScenario.owl#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:envi="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:kern="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
    xmlns:miss="http://www.anr-proteus.fr/sites/default/files/download/Ontology/mission.owl#"
    xmlns:info="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#"
  xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/MilitaryUnmannedAerialVehiclesScenario.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment xml:lang="en">This work has been funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project. It remains the property of the partners of the PROTEUS consortium but is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
    <rdfs:comment xml:lang="en">This scenario describes an airfield attack thanks to a package of unmanned aircraft.</rdfs:comment>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Proteus.owl"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >version delivered 1.0</owl:versionInfo>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#hosts">
    <owl:inverseOf>
      <rdf:Description rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#executesOn">
        <owl:inverseOf rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#hosts"/>
      </rdf:Description>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware"/>
    <rdfs:range rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Software"/>
  </owl:ObjectProperty>
  <kern:AutomataState rdf:ID="TacticalNavigationPhase"/>
  <kern:Environment rdf:ID="Environment">
    <kern:aggregates>
      <syst:ControlStationSystem rdf:ID="GroundControlStation">
        <kern:triggers>
          <kern:Interaction rdf:ID="RobotControl"/>
        </kern:triggers>
        <rdf:type rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#CompositeSystem"/>
        <kern:aggregates>
          <kern:CompositeSystem rdf:ID="GCSPlatform">
            <kern:aggregates>
              <kern:Hardware rdf:ID="IHSConsole"/>
            </kern:aggregates>
          </kern:CompositeSystem>
        </kern:aggregates>
      </syst:ControlStationSystem>
    </kern:aggregates>
    <kern:definesAFrame>
      <kern:Frame rdf:ID="WGS84ref"/>
    </kern:definesAFrame>
  </kern:Environment>
  <miss:MissionStateMachine rdf:ID="MissionFSM"/>
  <kern:AutomataState rdf:ID="AttackPhase"/>
  <miss:Mission rdf:ID="UAVPackageMission"/>
  <kern:CompositeSystem rdf:ID="UcavSoftwareSystem">
    <kern:aggregates>
      <syst:MissionManagementSystem rdf:ID="MissionManagementSystem"/>
    </kern:aggregates>
    <kern:aggregates>
      <syst:MissionManagementSystem rdf:ID="WeaponManagementSystem"/>
    </kern:aggregates>
    <kern:aggregates>
      <syst:MissionManagementSystem rdf:ID="NavigationManagementSystem"/>
    </kern:aggregates>
    <kern:aggregates>
      <syst:MissionManagementSystem rdf:ID="SensorManagementSystem"/>
    </kern:aggregates>
  </kern:CompositeSystem>
  <kern:AutomataState rdf:ID="FebaCrossingPhase"/>
  <kern:AutomataEndState rdf:ID="LandingPhase"/>
  <kern:AutomataState rdf:ID="DomesticsPhase"/>
  <kern:CompositeSystem rdf:ID="FCAS">
    <kern:aggregates rdf:resource="#GroundControlStation"/>
    <kern:aggregates>
      <kern:CompositeSystem rdf:ID="UcavPackage"/>
    </kern:aggregates>
  </kern:CompositeSystem>
  <kern:AutomataInitialState rdf:ID="TakeOfPhase"/>
  <robo:AirVehicle rdf:ID="UAV10"/>
  <robo:AirVehicle rdf:ID="UAV9"/>
  <syst:ObjectDetectionOrTrackingSensorHardware rdf:ID="RadarHardware">
    <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</kern:isAtomic>
  </syst:ObjectDetectionOrTrackingSensorHardware>
  <kern:CompositeSystem rdf:ID="UcavPlatform7">
    <kern:aggregates>
      <kern:CompositeSystem rdf:ID="UCAV7Payload">
        <kern:aggregates>
          <syst:WeaponHardware rdf:ID="BLG1000-1"/>
        </kern:aggregates>
        <kern:aggregates>
          <syst:WeaponHardware rdf:ID="BLG1000-2"/>
        </kern:aggregates>
        <rdf:type rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware"/>
      </kern:CompositeSystem>
    </kern:aggregates>
    <kern:aggregates>
      <kern:CompositeSystem rdf:ID="UCAV7Equipments">
        <rdf:type rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware"/>
        <kern:aggregates>
          <syst:CommunicationHardware rdf:ID="CommunicationHardware"/>
        </kern:aggregates>
        <kern:aggregates>
          <syst:MotorizationHardware rdf:ID="MotorizationHardware"/>
        </kern:aggregates>
        <kern:aggregates>
          <kern:Hardware rdf:ID="OnBoardComputer">
            <kern:hosts rdf:resource="#MissionManagementSystem"/>
            <kern:hosts rdf:resource="#WeaponManagementSystem"/>
            <kern:hosts rdf:resource="#NavigationManagementSystem"/>
            <kern:hosts rdf:resource="#SensorManagementSystem"/>
            <kern:triggers>
              <kern:Interaction rdf:ID="PlatformManagement">
                <kern:impacts rdf:resource="#UcavPlatform7"/>
              </kern:Interaction>
            </kern:triggers>
          </kern:Hardware>
        </kern:aggregates>
      </kern:CompositeSystem>
    </kern:aggregates>
  </kern:CompositeSystem>
</rdf:RDF>

