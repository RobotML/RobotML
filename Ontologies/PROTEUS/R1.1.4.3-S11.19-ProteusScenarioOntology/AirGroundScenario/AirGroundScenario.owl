<?xml version="1.0"?>
<rdf:RDF
    xmlns:syst="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Systems.owl#"
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
    xmlns:robo="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Robot.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:envi="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Environment.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:expe="http://www.anr-proteus.fr/sites/default/files/download/Ontology/experiment.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:kern="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#"
    xmlns:airg="http://www.anr-proteus.fr/sites/default/files/download/Ontology/AirGroundScenario.owl#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
    xmlns:miss="http://www.anr-proteus.fr/sites/default/files/download/Ontology/mission.owl#"
    xmlns:info="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#"
  xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/AirGroundScenario.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment xml:lang="en">This work has been funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project. It remains the property of the partners of the PROTEUS consortium but is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
    <rdfs:comment xml:lang="en">This ontology tries to use the whole set of proteus ontologies stack in order to describe a scenario encompassing aircraft collaborating with a car in order to track people in a forbidden area.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >version delivered 1.0</owl:versionInfo>
    <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/Proteus.owl"/>
  </owl:Ontology>
  <miss:Mission rdf:ID="ArmyMission">
    <miss:isDescribedBy>
      <miss:Task rdf:ID="ToKeepWatchOn"/>
    </miss:isDescribedBy>
    <miss:isDescribedBy>
      <miss:Task rdf:ID="ToDetect"/>
    </miss:isDescribedBy>
    <miss:isDescribedBy>
      <miss:Task rdf:ID="ToControl"/>
    </miss:isDescribedBy>
  </miss:Mission>
  <kern:Environment rdf:ID="EverythingAtCaylus">
    <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</kern:isAtomic>
    <kern:aggregates>
      <expe:Problem rdf:ID="SearchAndTrackProblem">
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXP030YP080">
            <kern:isPositionedIn>
              <kern:Frame rdf:ID="StartPointReferential"/>
            </kern:isPositionedIn>
            <rdfs:comment xml:lang="en">Building at X=30 Y=80 from start point.</rdfs:comment>
          </envi:Building>
        </kern:aggregates>
        <kern:aggregates>
          <syst:RoboticSystem rdf:ID="NonDecisionalPartOfRTrooper"/>
        </kern:aggregates>
        <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >false</kern:isAtomic>
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXM025YP025">
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
            <rdfs:comment xml:lang="en">Building at X=-25 Y=25 from start point.</rdfs:comment>
          </envi:Building>
        </kern:aggregates>
        <kern:aggregates>
          <syst:RoboticSystem rdf:ID="NonDecisionalPartOfRmax"/>
        </kern:aggregates>
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXP080YP075">
            <rdfs:comment xml:lang="en">Building at X=80 Y=75 from start point.</rdfs:comment>
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
          </envi:Building>
        </kern:aggregates>
        <kern:aggregates>
          <envi:LandSurface rdf:ID="CaylusLandSurface">
            <kern:definesAReferential rdf:resource="#StartPointReferential"/>
          </envi:LandSurface>
        </kern:aggregates>
        <kern:aggregates>
          <envi:Pedestrian rdf:ID="Intruder">
            <kern:isAnimatedByEvolutionModel>
              <miss:MissionStateMachine rdf:ID="IntruderExpectedBehavior">
                <kern:startsFrom>
                  <kern:AutomataInitialState rdf:ID="CrossingState">
                    <kern:hasFSMActivity>
                      <kern:FSMActivity rdf:ID="Crossing"/>
                    </kern:hasFSMActivity>
                  </kern:AutomataInitialState>
                </kern:startsFrom>
                <kern:animates rdf:resource="#Intruder"/>
                <kern:isDefinedBy>
                  <kern:Transition rdf:ID="CrossingToHidingTransition">
                    <kern:to>
                      <kern:AutomataState rdf:ID="HidingState">
                        <kern:hasFSMActivity>
                          <kern:FSMActivity rdf:ID="Hiding"/>
                        </kern:hasFSMActivity>
                      </kern:AutomataState>
                    </kern:to>
                    <kern:from rdf:resource="#CrossingState"/>
                  </kern:Transition>
                </kern:isDefinedBy>
                <kern:isDefinedBy rdf:resource="#HidingState"/>
              </miss:MissionStateMachine>
            </kern:isAnimatedByEvolutionModel>
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
          </envi:Pedestrian>
        </kern:aggregates>
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXP060YP080">
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
            <rdfs:comment xml:lang="en">Building at X=60 Y=80 from start point.</rdfs:comment>
          </envi:Building>
        </kern:aggregates>
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXP050YP050">
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
            <rdfs:comment xml:lang="en">Building at X=50 Y=50 from start point.</rdfs:comment>
          </envi:Building>
        </kern:aggregates>
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXM010YP030">
            <rdfs:comment xml:lang="en">Building at X=-10 Y=30 from start point.</rdfs:comment>
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
          </envi:Building>
        </kern:aggregates>
        <kern:aggregates>
          <envi:Building rdf:ID="BuildingXP010YP075">
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
            <rdfs:comment xml:lang="en">Building at X=10 Y=75 from start point.</rdfs:comment>
          </envi:Building>
        </kern:aggregates>
      </expe:Problem>
    </kern:aggregates>
    <kern:aggregates>
      <expe:Solution rdf:ID="DistributedDecisionalSystem">
        <kern:aggregates>
          <kern:Application rdf:ID="OnGroundDecision"/>
        </kern:aggregates>
        <kern:aggregates>
          <kern:Application rdf:ID="DecisionalPartOfRTrooper"/>
        </kern:aggregates>
        <kern:aggregates>
          <kern:Application rdf:ID="DecisionalPartOfRmax"/>
        </kern:aggregates>
        <rdf:type rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#CompositeSystem"/>
        <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >false</kern:isAtomic>
      </expe:Solution>
    </kern:aggregates>
    <kern:aggregates>
      <kern:CompositeSystem rdf:ID="RobotTeam">
        <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >false</kern:isAtomic>
        <kern:aggregates>
          <robo:AirVehicle rdf:ID="Rmax">
            <rdf:type rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#CompositeSystem"/>
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
            <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >false</kern:isAtomic>
            <kern:isAnimatedByEvolutionModel>
              <kern:EvolutionModel rdf:ID="RmaxEvolutionModel">
                <kern:animates rdf:resource="#Rmax"/>
              </kern:EvolutionModel>
            </kern:isAnimatedByEvolutionModel>
          </robo:AirVehicle>
        </kern:aggregates>
        <kern:aggregates>
          <robo:Car-likeVehicle rdf:ID="RTrooper">
            <kern:isAnimatedByEvolutionModel>
              <kern:EvolutionModel rdf:ID="R-TrooperEvolutionModel">
                <kern:animates rdf:resource="#RTrooper"/>
              </kern:EvolutionModel>
            </kern:isAnimatedByEvolutionModel>
            <kern:isPositionedIn rdf:resource="#StartPointReferential"/>
            <rdf:type rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#CompositeSystem"/>
            <kern:isAtomic rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >false</kern:isAtomic>
            <kern:aggregates>
              <syst:LocalizationSensorHardware rdf:ID="GPSSensorOfRTrooper"/>
            </kern:aggregates>
          </robo:Car-likeVehicle>
        </kern:aggregates>
      </kern:CompositeSystem>
    </kern:aggregates>
    <rdfs:comment xml:lang="en">Aggregation of robot team is anecdotic.</rdfs:comment>
  </kern:Environment>
  <syst:ImageSensorHardware rdf:ID="CameraOfRmax"/>
  <kern:FSMActivity rdf:ID="Controlling"/>
  <kern:Condition rdf:ID="LoftBarnOrShedReachedCondition">
    <kern:evaluatesToTruth>
      <kern:Event rdf:ID="LoftBarnOrShedReachedTrunsToTrue">
        <kern:entail rdf:resource="#CrossingToHidingTransition"/>
      </kern:Event>
    </kern:evaluatesToTruth>
  </kern:Condition>
  <miss:Mission rdf:ID="IntruderMission">
    <miss:isDescribedBy rdf:resource="#IntruderExpectedBehavior"/>
    <miss:isDescribedBy>
      <miss:Task rdf:ID="ToHide"/>
    </miss:isDescribedBy>
    <miss:isDescribedBy>
      <miss:Task rdf:ID="ToEscape"/>
    </miss:isDescribedBy>
    <miss:isDescribedBy>
      <miss:Task rdf:ID="ToCross"/>
    </miss:isDescribedBy>
  </miss:Mission>
  <kern:CompositeSystem rdf:ID="Solution_1"/>
  <kern:FSMActivity rdf:ID="Escaping"/>
  <kern:FSMActivity rdf:ID="Detecting"/>
  <kern:FSMActivity rdf:ID="KeepingWatchOn"/>
  <miss:Mission rdf:ID="RobotTeamMission">
    <miss:isDescribedBy rdf:resource="#ToDetect"/>
    <miss:isDescribedBy rdf:resource="#ToKeepWatchOn"/>
    <miss:hasRessource rdf:resource="#Rmax"/>
    <miss:hasRessource rdf:resource="#RTrooper"/>
  </miss:Mission>
  <owl:AllDifferent/>
</rdf:RDF>

