<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
    <!ENTITY swrla "http://swrl.stanford.edu/ontologies/3.3/swrla.owl#" >
    <!ENTITY sqwrl "http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#" >
    <!ENTITY kern "http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#" >
    <!ENTITY simu "http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#" >
    <!ENTITY info "http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#" >
]>


<rdf:RDF xmlns="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#"
     xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:simu="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#"
     xmlns:kern="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#"
     xmlns:info="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#"
     xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">
    <owl:Ontology rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl">
        <owl:versionInfo rdf:datatype="&xsd;string">version delivered 1.0</owl:versionInfo>
        <rdfs:comment xml:lang="en">FaCT++ does not work on this ontology. HermiT shall be used for reasoning.</rdfs:comment>
        <rdfs:comment xml:lang="en">The simulation ontology evolved from the specifications developped by experts in the field of robotics, simulation and modelling tooling. It uses the kernel and informations ontologies as its sole resources.</rdfs:comment>
        <rdfs:comment xml:lang="en">This work is based upon a previous one funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project (see http://www.anr-project.fr). It is the property of its authors and is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
        <rdfs:comment xml:lang="en">Version for Protégé 4.x flavors.</rdfs:comment>
        <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl"/>
        <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#aggregates -->

    <rdf:Description rdf:about="&kern;aggregates">
        <rdfs:comment xml:lang="en">indicates that systems will be aggregated in order to create a composite system</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#produces -->

    <rdf:Description rdf:about="&kern;produces">
        <rdfs:range rdf:resource="&kern;Information"/>
        <rdfs:domain rdf:resource="&kern;Probe"/>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#Partitions -->

    <owl:ObjectProperty rdf:about="&simu;Partitions">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">This property states that the Time domain function partitions the set obtained thanks to the Time domain property when it is feasable.</rdfs:comment>
        <rdfs:range rdf:resource="&simu;PropertyTimeDomain"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#canBeAssociatedTo -->

    <owl:ObjectProperty rdf:about="&simu;canBeAssociatedTo">
        <rdfs:comment xml:lang="en">Some of the simulated objects with a correspondance to a physical object in the real world can be associated to a 3D representation in the simulation.</rdfs:comment>
        <rdfs:range rdf:resource="&info;Object3D"/>
        <rdfs:domain rdf:resource="&simu;SimulatedSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#canBeDeployedIn -->

    <owl:ObjectProperty rdf:about="&simu;canBeDeployedIn">
        <rdfs:comment xml:lang="en">This property, sub property of Models, specifies that any system can be deployed in a simulator.</rdfs:comment>
        <rdfs:domain rdf:resource="&kern;System"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isModelledBy"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&simu;HardwareSimulator"/>
                    <rdf:Description rdf:about="&simu;HybridSimulator"/>
                    <rdf:Description rdf:about="&simu;SoftwareSimulator"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#creates -->

    <owl:ObjectProperty rdf:about="&simu;creates">
        <rdfs:comment xml:lang="en">this property expresses the fact some object can create information</rdfs:comment>
        <rdfs:range rdf:resource="&simu;Simulation"/>
        <rdfs:domain rdf:resource="&simu;SimulatorEvolutionModel"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasGlobalStateTransitionFunction -->

    <owl:ObjectProperty rdf:about="&simu;hasGlobalStateTransitionFunction">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasFunction"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasInput -->

    <owl:ObjectProperty rdf:about="&simu;hasInput">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Set used as source of the system specification</rdfs:comment>
        <rdfs:range rdf:resource="&info;Set"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasSet"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasInputSegments -->

    <owl:ObjectProperty rdf:about="&simu;hasInputSegments">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">this property explicits the fact that there is a need to specify the time segments used over the input set. The input set is the set as defined by the property simulation:hasInputSet. The set of input segment is called OMEGA.</rdfs:comment>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
        <rdfs:subPropertyOf rdf:resource="&simu;hasInput"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasInputSet -->

    <owl:ObjectProperty rdf:about="&simu;hasInputSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The set of the different inputs</rdfs:comment>
        <rdfs:range rdf:resource="&info;Set"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
        <rdfs:subPropertyOf rdf:resource="&simu;hasInput"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasModelReferenceSet -->

    <owl:ObjectProperty rdf:about="&simu;hasModelReferenceSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasSet"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasOutput -->

    <owl:ObjectProperty rdf:about="&simu;hasOutput">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Set used as destination of the system specification</rdfs:comment>
        <rdfs:range rdf:resource="&info;Set"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasSet"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasOutputFunction -->

    <owl:ObjectProperty rdf:about="&simu;hasOutputFunction">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasFunction"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasOutputSet -->

    <owl:ObjectProperty rdf:about="&simu;hasOutputSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">the set used as output</rdfs:comment>
        <rdfs:range rdf:resource="&info;Set"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
        <rdfs:subPropertyOf rdf:resource="&simu;hasOutput"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasPointOfViewConfiguration -->

    <owl:ObjectProperty rdf:about="&simu;hasPointOfViewConfiguration">
        <rdfs:comment xml:lang="en">null</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;SimulatedSceneCamera"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&info;Orientation3DEuler"/>
                    <rdf:Description rdf:about="&info;Position3DCartesian"/>
                    <rdf:Description rdf:about="&simu;Frustum"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasRelation -->

    <owl:ObjectProperty rdf:about="&simu;hasRelation">
        <rdfs:comment xml:lang="en">A relation establishes a property that the elements of a set have to respect. This property allows to &quot;structure&quot; the set associated.</rdfs:comment>
        <rdfs:range rdf:resource="&info;Relation"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasSimulationOrientation -->

    <owl:ObjectProperty rdf:about="&simu;hasSimulationOrientation">
        <rdfs:range rdf:resource="&info;Orientation"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;SimulatedPhysicalSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasSimulationPosition -->

    <owl:ObjectProperty rdf:about="&simu;hasSimulationPosition">
        <rdfs:range rdf:resource="&info;Position"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;SimulatedPhysicalSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasStateSet -->

    <owl:ObjectProperty rdf:about="&simu;hasStateSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasSet"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasSystemSet -->

    <owl:ObjectProperty rdf:about="&simu;hasSystemSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&info;hasSet"/>
        <rdfs:range rdf:resource="&simu;SystemSet"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasTimeBase -->

    <owl:ObjectProperty rdf:about="&simu;hasTimeBase">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">This property explains that any of the specification must be associated to at least one time base</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&info;hasSet"/>
        <rdfs:domain rdf:resource="&simu;SystemSpecification"/>
        <rdfs:range rdf:resource="&simu;TimeBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasTimeDomainFunctionSet -->

    <owl:ObjectProperty rdf:about="&simu;hasTimeDomainFunctionSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&info;Set"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasTimeFactor -->

    <owl:ObjectProperty rdf:about="&simu;hasTimeFactor">
        <rdfs:comment xml:lang="en">A render engine or a physics engine have a time factor.</rdfs:comment>
        <rdfs:range rdf:resource="&info;Real"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;GraphicsEngine"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasTimeSample -->

    <owl:ObjectProperty rdf:about="&simu;hasTimeSample">
        <rdfs:comment xml:lang="en">Time base has time samples.</rdfs:comment>
        <rdfs:range rdf:resource="&kern;Time"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;TimeBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasTimeStep -->

    <owl:ObjectProperty rdf:about="&simu;hasTimeStep">
        <rdfs:comment xml:lang="en">An render engine or a physics engine have a time step.</rdfs:comment>
        <rdfs:range rdf:resource="&info;Real"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;GraphicsEngine"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasTrajectory -->

    <owl:ObjectProperty rdf:about="&simu;hasTrajectory">
        <rdfs:comment xml:lang="en"></rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&simu;SimulatedPedestrian"/>
        <rdfs:range rdf:resource="&simu;Trajectory"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#hasValues -->

    <owl:ObjectProperty rdf:about="&simu;hasValues">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">This property only indicates that a set is composed of values that are instances of the info:data</rdfs:comment>
        <rdfs:domain rdf:resource="&info;Set"/>
        <rdfs:range rdf:resource="&kern;Data"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isAssociatedTo -->

    <owl:ObjectProperty rdf:about="&simu;isAssociatedTo">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">This association states that a relation is between elements of the same set and thus associated to only one set.</rdfs:comment>
        <rdfs:range rdf:resource="&info;Set"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isComposedOfAlgorithmicModule -->

    <owl:ObjectProperty rdf:about="&simu;isComposedOfAlgorithmicModule">
        <rdfs:comment xml:lang="en">this property allows an application to use module meaning also libraires and thus linking external libraries to application and to its inherited simulator</rdfs:comment>
        <rdfs:range rdf:resource="&kern;AlgorithmicModule"/>
        <rdfs:domain rdf:resource="&kern;Application"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isComposedOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isComposedOfElements -->

    <owl:ObjectProperty rdf:about="&simu;isComposedOfElements">
        <rdfs:comment xml:lang="en">This trajectory is composed of trajectory elements which represent the keyframes along the path.</rdfs:comment>
        <rdfs:range rdf:resource="&info;TrajectoryElement"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isComposedOf"/>
        <rdfs:domain rdf:resource="&simu;TrajectoryKeyFrames"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isCreatedBy -->

    <owl:ObjectProperty rdf:about="&simu;isCreatedBy">
        <rdfs:comment xml:lang="en">This property allows to link Data to Probe</rdfs:comment>
        <rdfs:domain rdf:resource="&simu;Simulation"/>
        <rdfs:range rdf:resource="&simu;SimulatorEvolutionModel"/>
        <owl:inverseOf rdf:resource="&simu;creates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isPartitionedBy -->

    <owl:ObjectProperty rdf:about="&simu;isPartitionedBy">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">This property states that the set obtained through the Time domain property is partitionnedd by the time domain function.</rdfs:comment>
        <rdfs:range rdf:resource="&info;Function"/>
        <owl:inverseOf rdf:resource="&simu;Partitions"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isRepresentedBy -->

    <owl:ObjectProperty rdf:about="&simu;isRepresentedBy">
        <rdfs:comment xml:lang="en">A 3D Object can be represented by several elements: meshes (one or several. e.g.: physical mesh and render mesh); texture mappings, object illumination ...</rdfs:comment>
        <rdfs:domain rdf:resource="&info;Object3D"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&simu;Illumination"/>
                    <rdf:Description rdf:about="&simu;Mesh"/>
                    <rdf:Description rdf:about="&simu;Textures"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#isSampleOf -->

    <owl:ObjectProperty rdf:about="&simu;isSampleOf">
        <rdfs:comment xml:lang="en">Times are samples of time base.</rdfs:comment>
        <rdfs:domain rdf:resource="&kern;Time"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isPropertyOf"/>
        <rdfs:range rdf:resource="&simu;TimeBase"/>
        <owl:inverseOf rdf:resource="&simu;hasTimeSample"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#mayCollaborateWith -->

    <owl:ObjectProperty rdf:about="&simu;mayCollaborateWith">
        <rdfs:comment xml:lang="en">this property explains that a simulator can collaborate with another application. An application being a software, this application can also be a simulator.</rdfs:comment>
        <rdfs:range rdf:resource="&kern;Application"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#simulatesAnimationOf -->

    <owl:ObjectProperty rdf:about="&simu;simulatesAnimationOf">
        <rdfs:comment xml:lang="en">this property is linked to animates property. It is the property allowing a simulator to fake animation of simulated systems.</rdfs:comment>
        <rdfs:range rdf:resource="&simu;SimulatedSystem"/>
        <rdfs:domain rdf:resource="&simu;SimulatorEvolutionModel"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#usesDiscreteTimeBase -->

    <owl:ObjectProperty rdf:about="&simu;usesDiscreteTimeBase">
        <rdfs:comment xml:lang="en">The engines in a 3D virtual world simulation use  a discrete time base to achieve the execution of the simulation.</rdfs:comment>
        <rdfs:range rdf:resource="&simu;DiscreteTimeBase"/>
        <rdfs:domain rdf:resource="&simu;GraphicsEngine"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#usesRenderSceneCamera -->

    <owl:ObjectProperty rdf:about="&simu;usesRenderSceneCamera">
        <rdfs:comment xml:lang="en">The render engine uses and manages the render camera of the scene. It controls the point of view of the 3D world.</rdfs:comment>
        <rdfs:domain rdf:resource="&simu;RenderEngine"/>
        <rdfs:range rdf:resource="&simu;SimulatedSceneCamera"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#withDeployementBecomes -->

    <owl:ObjectProperty rdf:about="&simu;withDeployementBecomes">
        <rdfs:comment xml:lang="en">expresses the fact that a Simulated system models specifically a system</rdfs:comment>
        <rdfs:domain rdf:resource="&kern;System"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isModelledBy"/>
        <rdfs:range rdf:resource="&simu;SimulatedSystem"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#Configuration -->

    <owl:DatatypeProperty rdf:about="&simu;Configuration">
        <rdfs:comment xml:lang="en">Configuration means to complete the simulator information in order to allow it to provide life to a system. At this step the configuration does not mean the configuration of the system to be simulated but information on the simulator itself, its distribution properties onto a hardware architecture, ...</rdfs:comment>
        <rdfs:domain rdf:resource="&simu;SimulatorEvolutionModel"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#EnvironmentMeshes -->

    <owl:DatatypeProperty rdf:about="&simu;EnvironmentMeshes">
        <rdfs:comment xml:lang="en">PhysicsEngines use Meshes in order to represent many parts of the environment. This Data type is inherited of what can be found in the Data tree of the information ontology</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#dimension -->

    <owl:DatatypeProperty rdf:about="&simu;dimension">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
