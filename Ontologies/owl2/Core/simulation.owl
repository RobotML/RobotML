        <owl:disjointWith rdf:resource="&simu;Trajectory"/>
        <rdfs:comment xml:lang="en">This relation states that v1 and v2 are related if they are segment (an element of OMEGAS) (not over the same set but using the same time base) and if the domain of the time base is identical for the two of them.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/simulation.owl#RenderEngine -->

    <owl:Class rdf:about="&simu;RenderEngine">
        <rdfs:subClassOf rdf:resource="&simu;GraphicsEngine"/>
