<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
    <!ENTITY swrla "http://swrl.stanford.edu/ontologies/3.3/swrla.owl#" >
    <!ENTITY sqwrl "http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#" >
    <!ENTITY kern "http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#" >
    <!ENTITY envi "http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#" >
]>


<rdf:RDF xmlns="http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#"
     xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:kern="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#"
     xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:envi="http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">
    <owl:Ontology rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl">
        <owl:versionInfo rdf:datatype="&xsd;string">version delivered 1.0</owl:versionInfo>
        <rdfs:comment xml:lang="en">Fact++ do not work with this ontology. HermiT shall be used.</rdfs:comment>
        <rdfs:comment xml:lang="en">This work is based upon a previous one funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project (see http://www.anr-project.fr). It is the property of its authors and is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
        <rdfs:comment xml:lang="en">Version for Protégé 4.x flavors.</rdfs:comment>
        <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
        <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl"/>
        <owl:imports rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#accelerates -->

    <owl:ObjectProperty rdf:about="&envi;accelerates">
        <rdfs:comment xml:lang="en">force accelerates physical object</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;Force"/>
        <rdfs:range rdf:resource="&kern;PhysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#forbidsMovement -->

    <owl:ObjectProperty rdf:about="&envi;forbidsMovement">
        <rdfs:comment>This relation is linked to the fact that some physical interactions are limited in their impact wrt movement of impacted Physical Object. For instance consdiering joint object they generally forbids movement other inside a specific subgroup of the euclidean displacements group.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;constrainsFrame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasAtmosphere -->

    <owl:ObjectProperty rdf:about="&envi;hasAtmosphere">
        <rdfs:comment xml:lang="en">Some planets have atmosphere</rdfs:comment>
        <rdfs:range rdf:resource="&envi;Atmosphere"/>
        <rdfs:domain rdf:resource="&envi;Planet"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasBuilding -->

    <owl:ObjectProperty rdf:about="&envi;hasBuilding">
        <rdfs:comment xml:lang="en">Planets such as Earth have buildings.</rdfs:comment>
        <rdfs:range rdf:resource="&envi;Building"/>
        <rdfs:domain rdf:resource="&envi;Planet"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasCurrentInFluid -->

    <owl:ObjectProperty rdf:about="&envi;hasCurrentInFluid">
        <rdfs:comment xml:lang="en">Atmospheres and waterbodies may have distribution of current in them and it is one of their physical characteristics</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;FluidObject"/>
        <rdfs:range rdf:resource="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#Tensor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasFloor -->

    <owl:ObjectProperty rdf:about="&envi;hasFloor">
        <rdfs:comment xml:lang="en">buildings have floors</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;Building"/>
        <rdfs:range rdf:resource="&envi;Floor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasLandSurface -->

    <owl:ObjectProperty rdf:about="&envi;hasLandSurface">
        <rdfs:comment xml:lang="en">A planet has a land surface whose vertical and horizontal dimensions characteirse the terrain.</rdfs:comment>
        <rdfs:range rdf:resource="&envi;LandSurface"/>
        <rdfs:domain rdf:resource="&envi;Planet"/>
        <rdfs:subPropertyOf rdf:resource="&envi;hasSurface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasStairs -->

    <owl:ObjectProperty rdf:about="&envi;hasStairs">
        <rdfs:comment xml:lang="en">Stairs are usually in buildings, but some stairs are outdoor.</rdfs:comment>
        <rdfs:range rdf:resource="&envi;Stairs"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&envi;Building"/>
                    <rdf:Description rdf:about="&envi;Planet"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasSurface -->

    <owl:ObjectProperty rdf:about="&envi;hasSurface">
        <rdfs:comment xml:lang="en">Physical objects have surfaces that determine their boudaries.</rdfs:comment>
        <rdfs:range rdf:resource="&envi;Surface"/>
        <owl:inverseOf rdf:resource="&envi;isSurfaceOf"/>
        <rdfs:domain rdf:resource="&kern;PhysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#hasWaterbody -->

    <owl:ObjectProperty rdf:about="&envi;hasWaterbody">
        <rdfs:comment xml:lang="en">Planets have waterbodies such as oceans, seas...</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;Planet"/>
        <rdfs:range rdf:resource="&envi;Waterbody"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#isLocatedOver -->

    <owl:ObjectProperty rdf:about="&envi;isLocatedOver">
        <rdfs:comment xml:lang="en">Buildings are located over land surface.</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;Building"/>
        <rdfs:range rdf:resource="&envi;LandSurface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#isSurfaceOf -->

    <owl:ObjectProperty rdf:about="&envi;isSurfaceOf">
        <rdfs:comment xml:lang="en">Surfaces are the borders of physical objects.</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;Surface"/>
        <rdfs:range rdf:resource="&kern;PhysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#DegreesOfFreedom -->

    <owl:DatatypeProperty rdf:about="&envi;DegreesOfFreedom">
        <kern:wikipediaURI>http://en.wikipedia.org/wiki/Degrees_of_freedom</kern:wikipediaURI>
        <rdfs:comment xml:lang="en">In many scientific fields, the degrees of freedom of a system is the number of parameters of the system that may vary independently. For example, the position of a figure in the plane has three degrees of freedom: its orientation and the two coordinates of any fixed point of the figure.</rdfs:comment>
        <rdfs:domain rdf:resource="&envi;Joint"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#AerodynamicForce -->

    <owl:Class rdf:about="&envi;AerodynamicForce">
        <rdfs:subClassOf rdf:resource="&envi;Force"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&kern;systemSource"/>
                <owl:allValuesFrom rdf:resource="&envi;Atmosphere"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&envi;Buoyancy"/>
        <owl:disjointWith rdf:resource="&envi;GravityForce"/>
        <owl:disjointWith rdf:resource="&envi;NormalForce"/>
        <rdfs:comment xml:lang="en">Aerodynamic force is the resultant force exerted on a body by the air (or some other gas) in which the body is immersed, and is due to the relative motion between the body and the fluid.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Atmosphere -->

    <owl:Class rdf:about="&envi;Atmosphere">
        <rdfs:subClassOf rdf:resource="&envi;FluidObject"/>
        <owl:disjointWith rdf:resource="&envi;Waterbody"/>
        <rdfs:comment xml:lang="en">a layer of gases that may surround a planet ]and that is held in place by the gravity of the planet. It is of interest if there are influences exercising on included physical objects.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Building -->

    <owl:Class rdf:about="&envi;Building">
        <rdfs:subClassOf rdf:resource="&envi;SolidObject"/>
        <owl:disjointWith rdf:resource="&envi;Planet"/>
        <owl:disjointWith rdf:resource="&envi;Surface"/>
        <owl:disjointWith rdf:resource="&kern;Agent"/>
        <owl:disjointWith rdf:resource="&kern;Environment"/>
        <owl:disjointWith rdf:resource="&kern;Hardware"/>
        <rdfs:comment xml:lang="en">human-made structure used or intended for supporting or sheltering any use or continuous occupancy</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Buoyancy -->

    <owl:Class rdf:about="&envi;Buoyancy">
        <rdfs:subClassOf rdf:resource="&envi;Force"/>
        <owl:disjointWith rdf:resource="&envi;GravityForce"/>
        <owl:disjointWith rdf:resource="&envi;NormalForce"/>
        <rdfs:comment xml:lang="en">Buoyancy is an upward acting force exerted by a fluid, that opposes an object&apos;s weight. This force is relevant for physical objects in waterbodies or at watersurface.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#CompleteBonding -->

    <owl:Class rdf:about="&envi;CompleteBonding">
        <rdfs:subClassOf rdf:resource="&envi;Joint"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&envi;DegreesOfFreedom"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This relation specifies that two PhysicalObject are so tightly linked that their frames are statically linked</rdfs:comment>
        <kern:wikipediaURI xml:lang="fr">http://fr.wikipedia.org/wiki/Liaison_%28m%C3%A9canique%29</kern:wikipediaURI>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Floor -->

    <owl:Class rdf:about="&envi;Floor">
        <rdfs:subClassOf rdf:resource="&envi;Surface"/>
        <owl:disjointWith rdf:resource="&envi;LandSurface"/>
        <owl:disjointWith rdf:resource="&envi;Stairs"/>
        <owl:disjointWith rdf:resource="&envi;WaterSurface"/>
        <rdfs:comment xml:lang="en">Walking surface of a room in a building.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#FluidObject -->

    <owl:Class rdf:about="&envi;FluidObject">
        <rdfs:subClassOf rdf:resource="&kern;PhysicalObject"/>
        <owl:disjointWith rdf:resource="&envi;SolidObject"/>
        <owl:disjointWith rdf:resource="&envi;Surface"/>
        <owl:disjointWith rdf:resource="&kern;Environment"/>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://sw.opencyc.org/concept/Mx4rvVjR45wpEbGdrcN5Y29ycA</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:datatype="&xsd;string">openCyc: FluidTangibleThing</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Fluid objects are physical objects that are able to deform and to flow</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Force -->

    <owl:Class rdf:about="&envi;Force">
        <rdfs:subClassOf rdf:resource="&kern;PhysicalInteraction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&kern;impacts"/>
                <owl:allValuesFrom rdf:resource="&kern;PhysicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&kern;systemSource"/>
                <owl:allValuesFrom rdf:resource="&kern;PhysicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&envi;Joint"/>
        <rdfs:comment xml:lang="en">A force is any influence that causes a free body to undergo a change in speed, a change in direction, or a change in shape. It is an interaction as it can be caused either by another body or as a synthesis of the environment influence (the two of them are systems).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#GravityForce -->

    <owl:Class rdf:about="&envi;GravityForce">
        <rdfs:subClassOf rdf:resource="&envi;Force"/>
        <owl:disjointWith rdf:resource="&envi;NormalForce"/>
        <rdfs:comment xml:lang="en">A force that is directly proportional to the PhysicalObject mass.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Joint -->

    <owl:Class rdf:about="&envi;Joint">
        <rdfs:subClassOf rdf:resource="&kern;PhysicalInteraction"/>
        <rdfs:comment xml:lang="en">In order to define Joint it is necessary to understand Mechanical linkage thus the following description.

A mechanical linkage is an assembly of bodies connected to manage forces and movement. The movement of a body, or link, is studied using geometry so the link is considered to be rigid. The connections between links are modeled as providing ideal movement, pure rotation or sliding for example, and are called joints. A linkage modeled as a network of rigid links and ideal joints is called a kinematic chain.

Linkages may be constructed from open chains, closed chains, or a combination of open and closed chains. Each link in a chain is connected by a joint to one or more other links. Thus, a kinematic chain can be modeled as a graph in which the links are paths and the joints are vertices, which is called a linkage graph.</rdfs:comment>
        <kern:wikipediaURI>http://en.wikipedia.org/wiki/Linkage_%28mechanical%29#Joints</kern:wikipediaURI>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#LandSurface -->

    <owl:Class rdf:about="&envi;LandSurface">
        <rdfs:subClassOf rdf:resource="&envi;Surface"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&envi;isSurfaceOf"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&envi;Planet"/>
                            <rdf:Description rdf:about="&envi;SolidObject"/>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&envi;Stairs"/>
        <owl:disjointWith rdf:resource="&envi;WaterSurface"/>
        <rdfs:comment xml:lang="en">The solid surface of a planet.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#NormalForce -->

    <owl:Class rdf:about="&envi;NormalForce">
        <rdfs:subClassOf rdf:resource="&envi;Force"/>
        <rdfs:comment xml:lang="en">The normal force is the repulsive force of interaction between atoms at close contact. This force is applicalble in hard surface contact of physical objects.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Pedestrian -->

    <owl:Class rdf:about="&envi;Pedestrian">
        <rdfs:subClassOf rdf:resource="&kern;Human"/>
        <rdfs:comment xml:lang="en">Physical Agent in a scenario which perfoms the action of going on foot, i.e. walking.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Planet -->

    <owl:Class rdf:about="&envi;Planet">
        <rdfs:subClassOf rdf:resource="&kern;PhysicalObject"/>
        <owl:disjointWith rdf:resource="&envi;Surface"/>
        <owl:disjointWith rdf:resource="&kern;Agent"/>
        <owl:disjointWith rdf:resource="&kern;Environment"/>
        <owl:disjointWith rdf:resource="&kern;Hardware"/>
        <rdfs:comment xml:lang="en">a celestial body  that is massive enough to be rounded by its own gravity</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#SolidObject -->

    <owl:Class rdf:about="&envi;SolidObject">
        <rdfs:subClassOf rdf:resource="&kern;PhysicalObject"/>
        <owl:disjointWith rdf:resource="&envi;Surface"/>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://sw.opencyc.org/concept/Mx4rvVjElJwpEbGdrcN5Y29ycA</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:datatype="&xsd;string">openCyc: SolidTangibleThing</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Stairs -->

    <owl:Class rdf:about="&envi;Stairs">
        <rdfs:subClassOf rdf:resource="&envi;Surface"/>
        <owl:disjointWith rdf:resource="&envi;WaterSurface"/>
        <rdfs:comment xml:lang="en">a construction designed to bridge a large vertical distance by dividing it into smaller vertical distances, called steps.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Surface -->

    <owl:Class rdf:about="&envi;Surface">
        <rdfs:subClassOf rdf:resource="&kern;PhysicalObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&envi;hasSurface"/>
                <owl:onClass rdf:resource="&envi;Surface"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&kern;Agent"/>
        <owl:disjointWith rdf:resource="&kern;Environment"/>
        <owl:disjointWith rdf:resource="&kern;Hardware"/>
        <rdfs:comment xml:lang="en">A two-dimensional topological manifold that is a boundary of a physical object</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#WaterSurface -->

    <owl:Class rdf:about="&envi;WaterSurface">
        <rdfs:subClassOf rdf:resource="&envi;Surface"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&envi;isSurfaceOf"/>
                <owl:someValuesFrom rdf:resource="&envi;Waterbody"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Surface of an ocean, a sea, a lake or an artificial reservatory.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#Waterbody -->

    <owl:Class rdf:about="&envi;Waterbody">
        <rdfs:subClassOf rdf:resource="&envi;FluidObject"/>
        <rdfs:comment xml:lang="en">Significant accumulation of water. It is of interest if there are influences exercising on included physical objects.
This concept IS NOT the &quot;Waterbody concept&quot; as defined in openCyc and that groups everything that is related to our waterbody concept.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Agent -->

    <rdf:Description rdf:about="&kern;Agent">
        <rdfs:subClassOf rdf:resource="&envi;SolidObject"/>
        <rdfs:comment xml:lang="en">TODO: relationship with openCyc was not possible. TBD perhaps.</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment -->

    <rdf:Description rdf:about="&kern;Environment"/>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware -->

    <rdf:Description rdf:about="&kern;Hardware">
        <rdfs:subClassOf rdf:resource="&envi;SolidObject"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Rules
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description rdf:about="urn:swrl#z">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="urn:swrl#a">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="urn:swrl#b">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="urn:swrl#x">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="urn:swrl#y">
        <rdf:type rdf:resource="&swrl;Variable"/>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&swrl;Imp"/>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="&swrl;AtomList"/>
