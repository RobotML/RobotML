<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
    <!ENTITY sqwrl "http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#" >
    <!ENTITY kern "http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#" >
    <!ENTITY syst "http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#" >
    <!ENTITY info "http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#" >
]>


<rdf:RDF xmlns="http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#"
     xml:base="http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:kern="http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#"
     xmlns:info="http://www.anr-proteus.fr/sites/default/files/download/Ontology/information.owl#"
     xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:syst="http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl">
        <rdfs:comment xml:lang="en">This work is based upon a previous one funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project (see http://www.anr-project.fr). It is the property of its authors and is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
        <kern:Author xml:lang="en">creation PROTEUS consortium - 
modified Bruno Patin - 15/09/2013
- dependancy introduced onto Environment ontology (linked to engine impacts machine torque)</kern:Author>
        <rdfs:comment xml:lang="en">documentation of rules added:
1/ in a computational system that aggregates a computational hardware and a software, this software MUST executes on this specific computational hardware</rdfs:comment>
        <rdfs:comment xml:lang="en">this ontology explicits the different systems that will be found in robots either hardware or software. Its new classes will be included for their most parts into the kernel:system concept. 
Each time we introduced relationships between concepts defined in the system ontology, question is asked whether it should be put in the Robot ontology. This means that the user of this ontology is to do the same thing and if want to expand the list of concepts then he / she have to think iif it is not better placed elsewhere.</rdfs:comment>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl"/>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl"/>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl"/>
        <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#Timeframes -->

    <owl:ObjectProperty rdf:about="&syst;Timeframes">
        <rdfs:comment xml:lang="en">Clocks are providing timeframe to hardware</rdfs:comment>
        <rdfs:domain rdf:resource="&kern;Clock"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isPropertyOf"/>
        <rdfs:range rdf:resource="&syst;ComputationalSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#canBeCodedInMessage -->

    <owl:ObjectProperty rdf:about="&syst;canBeCodedInMessage">
        <rdfs:comment xml:lang="en"></rdfs:comment>
        <rdfs:range rdf:resource="&syst;MiddlewareMessage"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&syst;RoboticSystemConfigurationData"/>
                    <rdf:Description rdf:about="&syst;RoboticSystemInfoData"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#canCommunicateThrough -->

    <owl:ObjectProperty rdf:about="&syst;canCommunicateThrough">
        <rdfs:comment xml:lang="en">An Application can use specific and dedicated applications called Communicatio middleware to communicate</rdfs:comment>
        <rdfs:domain rdf:resource="&kern;Application"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isPropertyOf"/>
        <rdfs:range rdf:resource="&syst;CommunicationMiddleware"/>
        <owl:inverseOf rdf:resource="&syst;hasApplicationPool"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#controlsSystem -->

    <owl:ObjectProperty rdf:about="&syst;controlsSystem">
        <rdfs:comment xml:lang="en">Interaction between a Control System and a Robotic System in order to achieve the corresponding control process:  regulators will take part to get the final output command to send to the Hardware or software of the robotic system. This property is not mapped anymore on the generic system definition that tackles only</rdfs:comment>
        <rdfs:domain rdf:resource="&syst;ControlSystem"/>
        <rdfs:range rdf:resource="&syst;DriverDeviceSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasApplicationPool -->

    <owl:ObjectProperty rdf:about="&syst;hasApplicationPool">
        <rdfs:range rdf:resource="&kern;Application"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;CommunicationMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasClock -->

    <owl:ObjectProperty rdf:about="&syst;hasClock">
        <rdfs:comment xml:lang="en">Some hardware are using a specific piece of hardware modeling a timeframe and called a clock</rdfs:comment>
        <rdfs:range rdf:resource="&kern;Clock"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;ComputationalSystem"/>
        <owl:inverseOf rdf:resource="&syst;Timeframes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasConfigurationData -->

    <owl:ObjectProperty rdf:about="&syst;hasConfigurationData">
        <rdfs:comment xml:lang="en">Each one of the robotic components (actuators, sensors, etc) has some configuration parameter data.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;DriverDeviceSystem"/>
        <rdfs:range rdf:resource="&syst;RoboticSystemConfigurationData"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasImplementingHardware -->

    <owl:ObjectProperty rdf:about="&syst;hasImplementingHardware">
        <rdfs:comment xml:lang="en">Robotic functional system has implementing hardware.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;RoboticFunctionalSystem"/>
        <owl:inverseOf rdf:resource="&syst;isHarwareImplementationOf"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&syst;ActuatorHardware"/>
                    <rdf:Description rdf:about="&syst;CommunicationHardware"/>
                    <rdf:Description rdf:about="&syst;PowerHardware"/>
                    <rdf:Description rdf:about="&syst;SensorHardware"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasImplementingSoftware -->

    <owl:ObjectProperty rdf:about="&syst;hasImplementingSoftware">
        <rdfs:comment xml:lang="en">Robotic functional system has implementing software.</rdfs:comment>
        <rdfs:range rdf:resource="&kern;Software"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;RoboticFunctionalSystem"/>
        <owl:inverseOf rdf:resource="&syst;isSoftwareImplementationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasInfoData -->

    <owl:ObjectProperty rdf:about="&syst;hasInfoData">
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;DriverDeviceSystem"/>
        <rdfs:range rdf:resource="&syst;RoboticSystemInfoData"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasInput -->

    <owl:ObjectProperty rdf:about="&syst;hasInput">
        <rdfs:comment xml:lang="en">A control system or a commandable system can have some command orders as input of a desired action.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;Closed-LoopControlSystem"/>
        <rdfs:range rdf:resource="&syst;ControlCommandOrder"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasMethods -->

    <owl:ObjectProperty rdf:about="&syst;hasMethods">
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasNoise -->

    <owl:ObjectProperty rdf:about="&syst;hasNoise">
        <rdfs:comment xml:lang="en">A System has to deal with disruptions or perturbations known as Noise to process a signal. These perturbations are inherent in the data adquisition process or in the commandable processes, and they can be present, for instance, in a control system, actuator system, sensor system, etc...</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;Closed-LoopControlSystem"/>
        <rdfs:range rdf:resource="&syst;NoiseSignal"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasNoiseModel -->

    <owl:ObjectProperty rdf:about="&syst;hasNoiseModel">
        <rdfs:comment xml:lang="en">A distorted interaction due to a noise signal can be modelled by an Evolution model of the corresponding noise.</rdfs:comment>
        <rdfs:range rdf:resource="&kern;NoiseModel"/>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;NoiseSignal"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasOutput -->

    <owl:ObjectProperty rdf:about="&syst;hasOutput">
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;Closed-LoopControlSystem"/>
        <rdfs:range rdf:resource="&syst;ControlCommandOutput"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hasinterface -->

    <owl:ObjectProperty rdf:about="&syst;hasinterface">
        <rdfs:comment xml:lang="en">A robotic system has an interface to publish or subscribe to its corresponding robotic middleware.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;has"/>
        <rdfs:domain rdf:resource="&syst;DriverDeviceSystem"/>
        <rdfs:range rdf:resource="&syst;SystemInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#hostInteractions -->

    <owl:ObjectProperty rdf:about="&syst;hostInteractions">
        <rdfs:comment xml:lang="en">A RoboticMiddleware is an interoperability software that not only executes applications, send and triggers interactions between systems/components, but it also hosts the interactions of the robotic platform.</rdfs:comment>
        <rdfs:range rdf:resource="&kern;ApplicationInteraction"/>
        <rdfs:domain rdf:resource="&syst;RoboticMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isControlledBy -->

    <owl:ObjectProperty rdf:about="&syst;isControlledBy">
        <rdfs:comment xml:lang="en">A robotic system is a controllable system as such there could exists a function (without any concern on how it is implemented) that takes into account the control of this particular robotic system.</rdfs:comment>
        <rdfs:range rdf:resource="&syst;ControlSystem"/>
        <rdfs:domain rdf:resource="&syst;DriverDeviceSystem"/>
        <owl:inverseOf rdf:resource="&syst;controlsSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isHarwareImplementationOf -->

    <owl:ObjectProperty rdf:about="&syst;isHarwareImplementationOf">
        <rdfs:comment xml:lang="en">Hardware is implementation of robotic functional system.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&kern;isPropertyOf"/>
        <rdfs:range rdf:resource="&syst;RoboticFunctionalSystem"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&syst;ActuatorHardware"/>
                    <rdf:Description rdf:about="&syst;CommunicationHardware"/>
                    <rdf:Description rdf:about="&syst;PowerHardware"/>
                    <rdf:Description rdf:about="&syst;SensorHardware"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isProtectedBy -->

    <owl:ObjectProperty rdf:about="&syst;isProtectedBy">
        <rdfs:comment xml:lang="en">A robotic system is a controllable system as such there could exists a function (without any concern on how it is implemented) that takes into account the protection of this particular robotic system. Protection means that in case of abnormal behaviour of the robotic system considered, there will be interactions occurring that will allow it to go on working on degraded modes or to stop it completely in order not to be destryed.</rdfs:comment>
        <rdfs:domain rdf:resource="&syst;DriverDeviceSystem"/>
        <rdfs:range rdf:resource="&syst;SecuritySystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isRepresentedBy -->

    <owl:ObjectProperty rdf:about="&syst;isRepresentedBy">
        <rdfs:domain rdf:resource="&kern;State"/>
        <owl:inverseOf rdf:resource="&syst;isRepresentingA"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isRepresentingA -->

    <owl:ObjectProperty rdf:about="&syst;isRepresentingA">
        <rdfs:range rdf:resource="&kern;State"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isSoftwareImplementationOf -->

    <owl:ObjectProperty rdf:about="&syst;isSoftwareImplementationOf">
        <rdfs:comment xml:lang="en">Software is implementation of robotic functionnal system.</rdfs:comment>
        <rdfs:domain rdf:resource="&kern;Software"/>
        <rdfs:subPropertyOf rdf:resource="&kern;isPropertyOf"/>
        <rdfs:range rdf:resource="&syst;RoboticFunctionalSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#managesMiddlewareMessage -->

    <owl:ObjectProperty rdf:about="&syst;managesMiddlewareMessage">
        <rdfs:range rdf:resource="&syst;MiddlewareMessage"/>
        <rdfs:domain rdf:resource="&syst;SystemInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#mapsOnto -->

    <owl:ObjectProperty rdf:about="&syst;mapsOnto">
        <rdfs:comment xml:lang="en">A robotic functional system in order to exist must be mapped onto an existing robotic system. Either it is mapped through software or hardware implementation or a mix is not described into this property. Two robotic systems can be different but implement the saame functional system.</rdfs:comment>
        <rdfs:range rdf:resource="&syst;DriverDeviceSystem"/>
        <rdfs:domain rdf:resource="&syst;RoboticFunctionalSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#measuresOutput -->

    <owl:ObjectProperty rdf:about="&syst;measuresOutput">
        <rdfs:range rdf:resource="&syst;ControlCommandOutput"/>
        <rdfs:domain rdf:resource="&syst;ProprioceptiveSensorSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#protectsSystem -->

    <owl:ObjectProperty rdf:about="&syst;protectsSystem">
        <rdfs:comment xml:lang="en">Interaction between a Security System and a Robotic System (in general). This interaction guarantees the protection in different levels (network, criptography, control, etc...) of a robotic system. This property does not concern the generic system concept that tackles only with composition and interefacing properties.</rdfs:comment>
        <rdfs:range rdf:resource="&syst;DriverDeviceSystem"/>
        <rdfs:domain rdf:resource="&syst;SecuritySystem"/>
        <owl:inverseOf rdf:resource="&syst;isProtectedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#publish -->

    <owl:ObjectProperty rdf:about="&syst;publish">
        <rdfs:comment xml:lang="en">The output interface controller manages the publishment of dataflow from a robotic system to the corresponding robotic middleware.</rdfs:comment>
        <rdfs:domain rdf:resource="&syst;OutputSystemInterface"/>
        <rdfs:range rdf:resource="&syst;RoboticMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#subscribe -->

    <owl:ObjectProperty rdf:about="&syst;subscribe">
        <rdfs:domain rdf:resource="&syst;InputSystemInterface"/>
        <rdfs:range rdf:resource="&syst;RoboticMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#DegreesOfFreedom -->

    <owl:DatatypeProperty rdf:about="&syst;DegreesOfFreedom">
        <kern:wikipediaURI rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Degrees_of_freedom</kern:wikipediaURI>
        <rdfs:comment xml:lang="en">In many scientific fields, the degrees of freedom of a system is the number of parameters of the system that may vary independently. For example, the position of a figure in the plane has three degrees of freedom: its orientation and the two coordinates of any fixed point of the figure.</rdfs:comment>
        <rdfs:domain rdf:resource="&syst;Joint"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/system.owl#isActive -->

    <owl:DatatypeProperty rdf:about="&syst;isActive">
        <rdfs:comment xml:lang="en">null</rdfs:comment>
        <rdfs:domain rdf:resource="&syst;SensorSystem"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;boolean">false</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;boolean">true</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#MachineTorque -->

    <owl:Class rdf:about="http://www.anr-proteus.fr/sites/default/files/download/Ontology/environment.owl#MachineTorque"/>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Action -->

    <rdf:Description rdf:about="&kern;Action">
        <owl:disjointWith rdf:resource="&syst;NoiseSignal"/>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Application -->

    <rdf:Description rdf:about="&kern;Application">
        <owl:disjointWith rdf:resource="&syst;SystemInterface"/>
        <kern:TODO xml:lang="en">Application impacts only software through ApllicationInteraction and Driver that is an Application impacts Physical objects due to hardwaretosoftware and softwaretohardware interaction.

This is clearly a mistake to be repaired.</kern:TODO>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Clock -->

    <rdf:Description rdf:about="&kern;Clock">
        <owl:disjointWith rdf:resource="&syst;ActuatorHardware"/>
        <owl:disjointWith rdf:resource="&syst;CommunicationHardware"/>
        <owl:disjointWith rdf:resource="&syst;ControlStationSystem"/>
        <owl:disjointWith rdf:resource="&syst;HardwareImplementationOfController"/>
        <owl:disjointWith rdf:resource="&syst;PowerHardware"/>
        <owl:disjointWith rdf:resource="&syst;SensorHardware"/>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Driver -->

    <owl:Class rdf:about="&kern;Driver">
        <owl:disjointWith rdf:resource="&syst;RoboticApplication"/>
        <owl:disjointWith rdf:resource="&syst;RoboticMiddleware"/>
        <kern:TODO xml:lang="en">Driver should triggers interactions between software and physical world (see definition of RoboticSystem)</kern:TODO>
    </owl:Class>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Environment -->

    <rdf:Description rdf:about="&kern;Environment">
        <owl:disjointWith rdf:resource="&syst;CyberPhysicalSystem"/>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#FileSystem -->

    <rdf:Description rdf:about="&kern;FileSystem">
        <owl:disjointWith rdf:resource="&syst;CommunicationMiddleware"/>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Hardware -->

    <rdf:Description rdf:about="&kern;Hardware">
        <rdfs:comment xml:lang="en">The robotic System concept provides the tool to aggregate the driber onto its hardware counterpart.</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#OperatingSystem -->

    <rdf:Description rdf:about="&kern;OperatingSystem">
        <owl:disjointWith rdf:resource="&syst;SystemInterface"/>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#Software -->

    <rdf:Description rdf:about="&kern;Software">
        <rdfs:comment xml:lang="en">In this scope, &quot;Models&quot; property means &quot;Implements&quot;.</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.anr-proteus.fr/sites/default/files/download/Ontology/kernel.owl#SoftwareModule -->

    <rdf:Description rdf:about="&kern;SoftwareModule">
