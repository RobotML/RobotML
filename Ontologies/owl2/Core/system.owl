    <owl:Class rdf:about="&syst;ControlCommandOrder">
        <rdfs:subClassOf rdf:resource="&info;Command"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&kern;isCodedIn"/>
