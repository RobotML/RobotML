<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
    <!ENTITY swrla "http://swrl.stanford.edu/ontologies/3.3/swrla.owl#" >
    <!ENTITY sqwrl "http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#" >
    <!ENTITY robo "http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/robot.owl#" >
    <!ENTITY experiment "http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#" >
    <!ENTITY info "http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#" >
]>


<rdf:RDF xmlns="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#"
     xml:base="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:robo="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/robot.owl#"
     xmlns:info="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl#"
     xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:experiment="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">
    <owl:Ontology rdf:about="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl">
        <owl:versionInfo rdf:datatype="&xsd;string">version delivered 1.0</owl:versionInfo>
        <rdfs:comment xml:lang="en">FaCT++ does not work on this ontology. HermiT shall be used for reasoning.</rdfs:comment>
        <rdfs:comment xml:lang="en">This ontology describes the lifecycle process adopted in the PROTEUS project for the problem builders and solution providers</rdfs:comment>
        <rdfs:comment xml:lang="en">This work is based upon a previous one funded by the French Research agency ANR (Agence Nationale de la Recherche) through thePROTEUS project (see http://www.anr-project.fr). It is the property of its authors and is governed by the CECILL B V1 licence (see http://www.cecill.info for information and the associated Licence_CeCILL-B_V1-en.txt or Licence_CeCILL-B_V1-fr.txt files).</rdfs:comment>
        <rdfs:comment xml:lang="en">Version for Protégé 4.x flavors.</rdfs:comment>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/information.owl"/>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl"/>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/robot.owl"/>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl"/>
        <owl:imports rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl"/>
        <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
        <owl:imports rdf:resource="http://swrl.stanford.edu/ontologies/3.3/swrla.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#isHostedBy -->

    <owl:AnnotationProperty rdf:about="&experiment;isHostedBy">
        <rdfs:comment rdf:datatype="&xsd;string">The Software System of  a Robot can be completly or partly hosted by a Robotic Middleware.</rdfs:comment>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#Creates -->

    <owl:ObjectProperty rdf:about="&experiment;Creates">
        <rdfs:comment xml:lang="en">This property explains what is a provider. It is exactly the guy or the guys creating the problem or the solution.</rdfs:comment>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&experiment;Problem"/>
                    <rdf:Description rdf:about="&experiment;Solution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&experiment;ProblemProvider"/>
                    <rdf:Description rdf:about="&experiment;SolutionProvider"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#communicatesWith -->

    <owl:ObjectProperty rdf:about="&experiment;communicatesWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">The Proteus Simulator and the Robotic Middlewares communicate.</rdfs:comment>
        <owl:inverseOf rdf:resource="&experiment;communicatesWith"/>
        <rdfs:domain rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticMiddleware"/>
        <rdfs:range rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#constrains -->

    <owl:ObjectProperty rdf:about="&experiment;constrains">
        <rdfs:comment rdf:datatype="&xsd;string">A Scenario constrains the configuration of a Problem.</rdfs:comment>
        <rdfs:range rdf:resource="&experiment;Configuration"/>
        <rdfs:domain rdf:resource="&experiment;Scenario"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#evaluates -->

    <owl:ObjectProperty rdf:about="&experiment;evaluates">
        <rdfs:comment xml:lang="en">A problem provider evaluates Solutions provided to answer his/her problem.</rdfs:comment>
        <rdfs:domain rdf:resource="&experiment;ProblemProvider"/>
        <rdfs:range rdf:resource="&experiment;Solution"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#evaluatesUsing -->

    <owl:ObjectProperty rdf:about="&experiment;evaluatesUsing">
        <owl:versionInfo rdf:datatype="&xsd;string">TODO: question arises from the fact that someone else than the problem provider could evaluates a solution using other metrics. This role has been introduced but will not be described at this step.</owl:versionInfo>
        <rdfs:comment xml:lang="en">This property expresses the fact that to measure you need a metric. This metric is to be used by the one trying to understand the solution performances. It is the Problem provider.</rdfs:comment>
        <rdfs:range rdf:resource="&experiment;Metric"/>
        <rdfs:domain rdf:resource="&experiment;ProblemProvider"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#generates -->

    <owl:ObjectProperty rdf:about="&experiment;generates">
        <rdfs:comment xml:lang="en">this indicates the capability for the platform to generate a software simulator</rdfs:comment>
        <rdfs:domain rdf:resource="&experiment;RobotMLPlatform"/>
        <rdfs:range rdf:resource="&experiment;RobotMLSimulator"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#hasConfiguration -->

    <owl:ObjectProperty rdf:about="&experiment;hasConfiguration">
        <rdfs:comment rdf:datatype="&xsd;string">A Problem has a Configuration. The Solution being a part of the architecture described by the Problem, it also has a Configuration.</rdfs:comment>
        <owl:versionInfo rdf:datatype="&xsd;string">TODO: is it not a kern:has subproperty ? More than that, is it necessaryfor expe:Problem when introducing the restriction on the kern:uses property ?</owl:versionInfo>
        <rdfs:range rdf:resource="&experiment;Configuration"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&experiment;Problem"/>
                    <rdf:Description rdf:about="&experiment;Solution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#isHostedBy -->

    <owl:ObjectProperty rdf:about="&experiment;isHostedBy">
        <rdfs:comment rdf:datatype="&xsd;string">The Software System of  a Robot can be completly or partly hosted by a Robotic Middleware.</rdfs:comment>
        <rdfs:domain rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticFunctionalSystem"/>
        <rdfs:range rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="&experiment;isPartOf">
        <rdfs:comment rdf:datatype="&xsd;string">A Solution is a part of the Software System of a Robot</rdfs:comment>
        <rdfs:domain rdf:resource="&experiment;Solution"/>
        <rdfs:range rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticFunctionalSystem"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#manages -->

    <owl:ObjectProperty rdf:about="&experiment;manages">
        <rdfs:comment rdf:datatype="&xsd;string">The Proteus Simulator and Robotic Middleware manage Probes.</rdfs:comment>
        <rdfs:domain rdf:resource="&experiment;RobotMLSimulator"/>
        <rdfs:range rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#SoftwareProbe"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#providesChallengeEnvironmentTo -->

    <owl:ObjectProperty rdf:about="&experiment;providesChallengeEnvironmentTo">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">Specificity of the Challenge provider to provide the competition environnment to the challenger</rdfs:comment>
        <rdfs:domain rdf:resource="&experiment;ChallengeProvider"/>
        <rdfs:range rdf:resource="&experiment;Challenger"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#providesClockTo -->

    <owl:ObjectProperty rdf:about="&experiment;providesClockTo">
        <rdfs:comment rdf:datatype="&xsd;string">The Proteus Simulator provides a clock to the middleware it communicates with.</rdfs:comment>
        <rdfs:domain rdf:resource="&experiment;RobotMLSimulator"/>
        <rdfs:range rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#RoboticMiddleware"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#providesSolution -->

    <owl:ObjectProperty rdf:about="&experiment;providesSolution">
        <rdfs:comment rdf:datatype="&xsd;string">A Challenger provides a Solution to a Problem.</rdfs:comment>
        <rdfs:range rdf:resource="&experiment;Solution"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#usesChallengeEnvironment -->

    <owl:ObjectProperty rdf:about="&experiment;usesChallengeEnvironment">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">A challenger is part of the challenge envronment as provided by the provider of the challenge.</rdfs:comment>
        <rdfs:range rdf:resource="&experiment;ChallengeProvider"/>
        <rdfs:domain rdf:resource="&experiment;Challenger"/>
        <owl:inverseOf rdf:resource="&experiment;providesChallengeEnvironmentTo"/>
    </owl:ObjectProperty>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#isAnimatedByEvolution -->

    <owl:ObjectProperty rdf:about="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#isAnimatedByEvolution"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#Force -->

    <owl:Class rdf:about="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#Force">
        <owl:disjointWith rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Action"/>
        <owl:disjointWith rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#DriverToHardwareInteraction"/>
        <owl:disjointWith rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#HardwareToDriverInteraction"/>
        <owl:disjointWith rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#InterAgentCommunication"/>
        <owl:disjointWith rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/system.owl#NoiseSignal"/>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#Pedestrian -->

    <owl:Class rdf:about="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/environment.owl#Pedestrian">
        <owl:disjointWith rdf:resource="&experiment;ChallengeProvider"/>
        <owl:disjointWith rdf:resource="&experiment;Challenger"/>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#ChallengeProvider -->

    <owl:Class rdf:about="&experiment;ChallengeProvider">
        <rdfs:subClassOf rdf:resource="&experiment;ProblemProvider"/>
        <owl:disjointWith rdf:resource="&experiment;Challenger"/>
        <rdfs:comment xml:lang="en">A Challenge Provider is first a problem provider but he / she also provides the environment required for a competition to occur as for example real robots, arena, and so on and so forth.</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#Challenger -->

    <owl:Class rdf:about="&experiment;Challenger">
        <rdfs:subClassOf rdf:resource="&experiment;SolutionProvider"/>
        <rdfs:comment rdf:datatype="&xsd;string">The challenger is a solution provider that accepts to participate to a competion, a challenge as organised by a ChallengeProvider.</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#Configuration -->

    <owl:Class rdf:about="&experiment;Configuration">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#State"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#isUsedBy"/>
                <owl:someValuesFrom rdf:resource="&experiment;Problem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#AutomataState"/>
        <rdfs:comment rdf:datatype="&xsd;string">A configuration is a particular Initial state of a System</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#EditModel -->

    <owl:Class rdf:about="&experiment;EditModel">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Action"/>
        <rdfs:comment xml:lang="en">This action is linked to the capability of an actor to represent its environment. In this context, mainly it means using the robot ML platform to model robotic problems or solutions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#EnvironmentModel -->

    <owl:Class rdf:about="&experiment;EnvironmentModel">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#CompositeModel"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#models"/>
                <owl:allValuesFrom rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Environment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#models"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#EnvironmentSimulator -->

    <owl:Class rdf:about="&experiment;EnvironmentSimulator">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/simulation.owl#SoftwareSimulator"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#models"/>
                <owl:allValuesFrom rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Environment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:versionInfo rdf:datatype="&xsd;string">TODO: even if kern:models some kern:Environment, question remains to see if it is not more a expe:Environment Model</owl:versionInfo>
        <rdfs:comment xml:lang="en">This simulator goal is to implement a model of the environment into an application. Nevrtheless it models the environment.</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#GenerateSimulator -->

    <owl:Class rdf:about="&experiment;GenerateSimulator">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Action"/>
        <rdfs:comment xml:lang="en">a user of the robot modelling platform is creating a simulator through this action using the RobotML platform.</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#Metric -->

    <owl:Class rdf:about="&experiment;Metric">
        <rdfs:subClassOf rdf:resource="&info;Function"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&info;hasInput"/>
                <owl:allValuesFrom rdf:resource="&info;CrossProduct"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&info;hasSet"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&info;hasOutput"/>
                <owl:allValuesFrom rdf:resource="&info;OrderedSet"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#isCodedIn"/>
                <owl:allValuesFrom rdf:resource="&info;StructuredData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&info;Relation"/>
        <owl:disjointWith rdf:resource="&info;TrajectoryFunction"/>
        <rdfs:comment rdf:datatype="&xsd;string">A metric allows to measure from the data recorded using probes the performance of a particular system.</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#ModellingActivity -->

    <owl:Class rdf:about="&experiment;ModellingActivity">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#hasAction"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&experiment;EditModel"/>
                            <rdf:Description rdf:about="&experiment;GenerateSimulator"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Activity of an agent to model things</rdfs:comment>
    </owl:Class>
    


    <!-- http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/experiment.owl#Problem -->

    <owl:Class rdf:about="&experiment;Problem">
        <rdfs:subClassOf rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#CompositeSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://europe.bourges.univ-orleans.fr/sites/default/files/download/Ontology/Core/kernel.owl#aggregates"/>
                <owl:someValuesFrom rdf:resource="&experiment;RobotMLSimulator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
